<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="SmtbatSvc" kind="business" longname="文件批量服务" package="cn.sunline.ltts.busi.aptran.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="smtbat" name="smtbat" longname="提交批量文件服务">
        <interface>
            <input packMode="true">
                <field id="source" type="ApBaseEnumType.E_SYSCCD" required="false" multi="false" array="false" longname="来源系统标识"/>
                <field id="target" type="ApBaseEnumType.E_SYSCCD" required="false" multi="false" array="false" longname="目标系统标识"/>
                <field id="acctdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="业务开始日期"/>
                <field id="endtdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="业务结束日期"/>
                <field id="lsleix" type="BaseType.U_CHAR01" required="false" multi="false" array="false" longname="流水类型"/>
                <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="原机构号"/>
                <field id="crcycn" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种"/>
                <field id="dataid" type="ApBaseEnumType.E_FILETP" required="false" multi="false" array="false" longname="数据类型"/>
                <field id="busseq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="业务流水"/>
                <field id="issubf" type="BaseEnumType.E_YES___" required="false" default="0" multi="false" array="false" longname="是否子文件"/>
                <field id="tdcnno" type="BaseType.U_DCNNO" required="false" multi="false" array="false" longname="目标DCN号"/>
                <field id="subbno" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="批量交易子批次号"/>
                <field id="fileList" type="IoApFileBatchType.BatchFileSubmit" required="false" multi="true" array="false" longname="文件集合"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="busseq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="文件批次号"/>
                <field id="fileph" type="BaseType.U_FILEPH" required="false" multi="false" array="false" longname="文件路径"/>
                <field id="filena" type="BaseType.U_FILENA" required="false" multi="false" array="false" longname="文件名称"/>
                <field id="okfile" type="BaseType.U_FILENA" required="false" multi="false" array="false" longname="okfile"/>
                <field id="filesq" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="文件批次号"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="smtbatReturn" name="smtbatReturn" longname="子文件处理汇报">
        <interface>
            <input packMode="true">
                <field id="wjplsub" type="ApDefineType.ApWjplxxb" required="false" multi="false" array="false" longname="文件批量信息子表"/>
            </input>
            <output asParm="true" packMode="true"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="split" name="split" longname="拆分时文件子表建立">
        <interface>
            <input packMode="true">
                <field id="wjplxxb" type="ApDefineType.ApWjplxxb" required="false" multi="false" array="false" longname="批量交易信息表"/>
                <field id="tdcnno" type="BaseType.U_DCNNO" required="false" multi="false" array="false" longname="目标DCN号"/>
            </input>
            <output asParm="true" packMode="true"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="returnWithoutMerge" name="returnWithoutMerge" longname="子任务处理情况通知管理节点">
        <interface>
            <input packMode="true">
                <field id="wjplsub" type="ApDefineType.ApWjplxxb" required="false" multi="false" array="false" longname="文件批量信息子表"/>
            </input>
            <output asParm="true" packMode="true"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="batTranSmt" name="batTranSmt" longname="柜面文件批量交易提交处理">
        <interface>
            <input packMode="true">
                <field id="acctdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="业务日期"/>
                <field id="dataid" type="ApBaseEnumType.E_FILETP" required="false" multi="false" array="false" longname="数据类型"/>
                <field id="busseq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="业务流水"/>
                <field id="cdcnno" type="BaseType.U_DCNNO" required="false" multi="false" array="false" longname="DCN编号" ref="BaseDict.Comm.cdcnno"/>
                <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="机构号" ref="BaseDict.Comm.brchno"/>
                <field id="isrege" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否登记文件流水"/>
                <field id="btfile" type="IoApFileBatchType.BatchFileSubmit" required="false" multi="false" array="false" longname="文件信息"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="busseq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="文件批次号"/>
                <field id="fileph" type="BaseType.U_FILEPH" required="false" multi="false" array="false" longname="文件路径"/>
                <field id="filena" type="BaseType.U_FILENA" required="false" multi="false" array="false" longname="文件名称"/>
                <field id="okfile" type="BaseType.U_FILENA" required="false" multi="false" array="false" longname="okfile"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
