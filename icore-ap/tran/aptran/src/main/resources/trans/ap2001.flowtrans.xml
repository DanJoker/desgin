<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="ap2001" longname="根据账号查询所属法人" kind="1" package="cn.sunline.ltts.busi.aptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[法人无关交易，提供给外围系统根据账号确定所属的法人]]></description>
    <interface package="cn.sunline.ltts.busi.aptran.trans.intf">
        <input packMode="true">
            <field id="acctno" type="BaseType.U_ACCTNO" required="true" multi="false" array="false" longname="账号" ref="BaseDict.Comm.acctno"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="corpno" ref="BaseDict.Comm.corpno" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="法人代码"/>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvApAcctRout.GetAcctCorpno" id="GetAcctCorpno" longname="获取账户所属法人">
            <in_mappings by_interface="true">
                <mapping src="acctno" dest="acctno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="corpno" dest="corpno" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
