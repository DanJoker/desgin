<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="demote" longname="测试验证" kind="1" package="cn.sunline.edsp.busi.aptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.edsp.busi.aptran.trans.intf">
        <input packMode="false">
            <field id="parmcd" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="参数代码"/>
        </input>
        <output asParm="true" packMode="false">
            <field id="pmval1" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="参数值"/>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="My.queryParms" id="queryParms" longname="查询参数服务">
            <in_mappings by_interface="true">
                <mapping src="parmcd" dest="parmcd" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="pmval1" dest="pmval1" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
