<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<sqls xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="UpdAppAcdt" longname="跟新账务日期表" package="cn.sunline.edsp.busi.aptran.namedsql" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <update method="update" id="UpdAppAcdt" longname="跟新账务日期">
        <sql><![CDATA[
    	update
    	    	app_acdt 
    	set
    	    	validt=#newvalidt# ,
              updttm=#updttm#
    	where
    	    	validt=#oldvalidt# 
    	]]></sql>
        <parameterMap class="java.util.Map">
            <parameter property="oldvalidt" jdbcType="VARCHAR" javaType="BaseType.U_DATE19" mode="in" longname="旧的生效日期" parameterType="condition"/>
            <parameter property="newvalidt" jdbcType="VARCHAR" javaType="BaseType.U_DATE19" mode="in" longname="新的生效日期" parameterType="condition"/>
            <parameter property="updttm" jdbcType="VARCHAR" javaType="BaseType.U_DATE19" mode="in" longname="修改时间戳" parameterType="condition"/>
        </parameterMap>
    </update>
</sqls>
