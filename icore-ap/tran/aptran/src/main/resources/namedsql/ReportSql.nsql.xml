<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<sqls id="ReportSql" longname="调用报表存储过程" datasource="myreport" package="cn.sunline.ltts.busi.aptran.namedsql" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[调用报表存储过程]]></description>
    <update method="update" id="GetBaseData" longname="取基础数据">
        <sql type="mysql"><![CDATA[call Pkg_Rp_Genbase_Data.Genbase_Data(#trandt#)]]></sql>
        <sql type="oracle"><![CDATA[call Pkg_Rp_Genbase_Data.Genbase_Data(#trandt#)]]></sql>
        <parameterMap class="java.util.Map">
            <parameter property="trandt" ref="PbDict.Charge.trandt" jdbcType="VARCHAR" javaType="PbBaseType.U_DATE08" mode="in" longname="交易日期" parameterType="condition"/>
        </parameterMap>
    </update>
    <update method="update" id="prcMakeDate" longname="生成报表数据">
        <sql type="oracle" desc=""><![CDATA[call rp_migrate_data(#lastdt#.#currdt#.#erorcd#.#errmsg#)]]></sql>
        <sql type="mysql" desc=""><![CDATA[call rp_migrate_data(#lastdt#.#currdt#.#erorcd#.#errmsg#)]]></sql>
        <parameterMap class="java.util.Map">
            <parameter property="lastdt" ref="" jdbcType="VARCHAR" javaType="BaseType.U_DATE08" mode="in" longname="最后日期" parameterType="condition"/>
            <parameter property="currdt" jdbcType="VARCHAR" javaType="BaseType.U_DATE08" mode="in" longname="当前日期" parameterType="condition"/>
            <parameter property="erorcd" jdbcType="VARCHAR" javaType="integer" mode="in" longname="错误码" parameterType="condition"/>
            <parameter property="errmsg" jdbcType="VARCHAR" javaType="string" mode="in" longname="错误信息" parameterType="condition"/>
        </parameterMap>
    </update>
</sqls>
