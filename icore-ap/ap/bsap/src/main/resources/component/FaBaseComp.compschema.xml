            </interface>
            <exceptions/>
        </service>
        <service id="getBatchRemoteDir" longname="获取批量远程目录" name="getBatchRemoteDir">
            <interface>
                <input packMode="false"/>
                <output asParm="true" packMode="false">
                    <field id="field" type="string" required="false" multi="false" array="false" longname="批量文件远程目录"/>
                </output>
                <property packMode="false"/>
                <printer packMode="false"/>
            </interface>
            <exceptions/>
        </service>
        <service id="getRemoteFileList" longname="获取指定远程路径下指定规则文件列表" name="getRemoteFileList">
            <interface>
                <input packMode="false">
                    <field id="remoteDir" type="string" required="false" multi="false" array="false" longname="远程路径"/>
                    <field id="fileRegs" type="string" required="false" multi="false" array="false" longname="匹配规则"/>
                </input>
                <output asParm="true" packMode="false">
                    <field id="remoteFileList" type="ComFaApFile.ApRemoteFileList" required="false" multi="false" array="false" longname="远程文件列表"/>
                </output>
                <property packMode="true"/>
                <printer packMode="true"/>
            </interface>
            <exceptions/>
        </service>
    </componentType>
</componentSchema>
