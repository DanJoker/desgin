<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<componentSchema id="FileTransferImpl" package="cn.sunline.ltts.busi.aplt.transfer" longname="文件传输服务组件实现定义" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[文件传输服务组件实现定义]]></description>
    <componentImplementation id="AFTFileTransfer" longname="基于AFT的文件传输组件实现" type="BaseComp.FileTransfer">
        <implementation.java class="cn.sunline.ltts.busi.aplt.component.AFTFileTransferImpl"/>
        <properties>
            <property name="ip" type="string" longname="文件服务器IP地址"/>
            <property name="port" type="int" longname="文件服务器端口"/>
            <property name="maxThreads" type="int" default="5" longname="最大线程数"/>
            <property name="sysId" type="string" longname="系统渠道号"/>
            <property name="app" type="string" default="AFT" longname="应用码"/>
            <property name="uptranscode" type="string" default="UP" longname="上传交易码"/>
            <property name="downtranscode" type="string" default="DOWN" longname="下载交易码"/>
            <property name="localWorkDir" type="string" longname="本地路径"/>
        </properties>
    </componentImplementation>
    <componentImplementation id="ApacheFtpFileTransfer" longname="apache版本" type="FaBaseComp.FileTransfer">
        <implementation.java class="cn.sunline.ltts.busi.aplt.impl.FaBaseCompFtpFileTransferImpl"/>
        <properties>
            <property name="ip" type="string" longname="ftp地址"/>
            <property name="port" type="int" default="21" longname="ftp端口"/>
            <property name="user" type="string" longname="用户"/>
            <property name="password" type="string" longname="密码"/>
            <property name="connectTimeoutInMs" type="int" default="30000" longname="连接超时时间"/>
            <property name="binaryMode" type="boolean" default="true" longname="是否二进制传输"/>
            <property name="localWorkDir" type="string" longname="本地工作目录,必须以'/'结束"/>
            <property name="remoteWorkDir" type="string" longname="远程工作目录, 必须以'/'结束"/>
            <property name="retryInterval" type="int" longname="连接失败重连次数"/>
            <property name="retryTime" type="int" longname="连接失败重连间隔"/>
        </properties>
    </componentImplementation>
    <componentImplementation id="NFSFileTransfer" longname="基于NFS的网络文件系统" type="BaseComp.FileTransfer">
        <implementation.java class="cn.sunline.ltts.busi.aplt.component.transfer.NFSFileTransferImpl"/>
        <properties>
            <property name="localWorkDir" type="string" longname="本地工作目录,必须以'/'结束"/>
            <property name="remoteWorkDir" type="string" longname="远程工作目录, 必须以'/'结束"/>
        </properties>
    </componentImplementation>
</componentSchema>
