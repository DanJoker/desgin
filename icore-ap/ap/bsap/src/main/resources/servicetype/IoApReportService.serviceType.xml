<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoApReportService" kind="business" longname="批量后的汇报服务" package="cn.sunline.ltts.busi.bsap.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="AfterTransRport" name="AfterTransRport" longname="批量交易后的汇报服务">
        <interface>
            <input packMode="true">
                <field id="flowid" type="KBaseType.U_PLJYLCBS" required="true" multi="false" array="false" longname="批量日终流程ID"/>
                <field id="flowdt" type="BaseType.U_DATE08" required="true" multi="false" array="false" longname="批量日终管理日期"/>
                <field id="systcd" type="BaseType.U_SYSTCD" required="false" multi="false" array="false" longname="系统代码"/>
                <field id="corpno" type="BaseType.U_CORPNO" required="true" multi="false" array="false" longname="法人代码" ref="ApDict.Aplt.corpno"/>
                <field id="dcnnum" type="BaseType.U_DCNNO" required="true" default="0" multi="false" array="false" longname="DCN编号"/>
                <field id="sucess" type="BaseEnumType.E_TRUE__" required="false" multi="false" array="false" longname="交易是否成功"/>
                <field id="txntal" type="BaseType.U_LONGIN" required="true" default="0" multi="false" array="false" longname="有效交易个数"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="AfterProcessReport" name="AfterProcessReport" longname="批量流程后的汇报服务">
        <interface>
            <input packMode="true">
                <field id="flowid" type="KBaseType.U_PLJYLCBS" required="true" multi="false" array="false" longname="批量日终流程ID"/>
                <field id="systcd" type="BaseType.U_SYSTCD" required="false" multi="false" array="false" longname="系统代码"/>
                <field id="corpno" type="BaseType.U_CORPNO" required="true" multi="false" array="false" longname="法人代码" ref="ApDict.Aplt.corpno"/>
                <field id="flowdt" type="BaseType.U_DATE08" required="true" multi="false" array="false" longname="批量日终管理日期"/>
                <field id="flowtp" type="BaseEnumType.E_EODTP" required="false" multi="false" array="false" longname="批量流程分类"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
