<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoApDayendBatchQuery" kind="business" longname="日终批量查询相关" package="cn.sunline.ltts.busi.bsap.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<service id="QueryDayendBatchCount" name="queryDayendBatchCount" longname="查询节点有效日终批量交易数（ksys_jykzhq）">
        <interface>
            <input packMode="false">
            	<field id="cdcnno" type="BaseType.U_CHAR08" required="true" multi="false" default="adm" array="false" longname="DCN号"/>
            	<field id="xitongbs" type="KBaseType.U_LEIXIN10" required="true" multi="false" array="false" longname="系统标识号" />
	            <field id="farendma" type="KBaseType.U_FARENDMA" required="true" multi="false" array="false" longname="法人代码" />
	            <field id="pljylcbs" type="KBaseType.U_PLJYLCBS" required="true" multi="false" array="false" longname="批量流程ID" />
            </input>
            <output asParm="false" packMode="false">
                <field id="count" type="int" required="false" multi="false" array="false" longname="记录条数"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
