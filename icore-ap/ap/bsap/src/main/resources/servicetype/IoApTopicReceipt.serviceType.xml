<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoApTopicReceipt" kind="business" longname="发布订阅回执处理" package="cn.sunline.ltts.amsg.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="getTopicDefine" name="getTopicDefine" longname="获得主题定义">
        <interface>
            <input packMode="false">
                <field id="topcid" type="BaseType.U_CHAR20" required="false" multi="false" array="false" longname="消息主题ID" ref="ApDict.Aplt.topcid"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="detail" type="ApMessageComplexType.AppTopiDefiCType" required="false" multi="false" array="false" longname="主题定义"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="registSubscribers" name="registSubscribers" longname="登记订阅者">
        <interface>
            <input packMode="false">
            	<field id="mtinfo" type="TrxBaseEnvs.MessageTotalInfo" required="false" multi="false" array="false" longname="消息信息"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="subscriberProcess" name="subscriberProcess" longname="订阅者处理">
        <interface>
            <input packMode="false">
            	<field id="mtinfo" type="TrxBaseEnvs.MessageTotalInfo" required="false" multi="false" array="false" longname="消息信息"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="receiptPushlisher" name="receiptPushlisher" longname="回执处理服务">
        <interface>
            <input packMode="false">
                <field id="affaid" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="事务ID"/>
                <field id="tacorp" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="目标法人"/>
                <field id="tardcn" type="BaseType.U_DCNNO" required="false" multi="false" array="false" longname="目标DCN"/>
                <field id="tsyscd" type="BaseType.U_SYSTCD" required="false" multi="false" array="false" longname="目标系统"/>
                <field id="status" type="ApBaseEnumType.E_SYNCST" required="false" multi="false" array="false" longname="状态"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
