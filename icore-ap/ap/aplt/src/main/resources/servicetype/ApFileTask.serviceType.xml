<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="ApFileTask" kind="business" longname="文件批量服务" package="cn.sunline.ltts.busi.aplt.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[文件批量服务]]></description>
    <service id="FileBatchExecutTaskSubmit" name="fileBatchExecutTaskSubmit" longname="批量文件处理任务提交服务">
        <interface>
            <input packMode="false">
                <field id="entrno" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="委托号" ref="BaseDict.Comm.entrno"/>
                <field id="btflmd" type="BaseType.U_CHAR04" required="false" multi="false" array="false" longname="批量文件类型" desc="文件类型" ref="BaseDict.Comm.btflmd"/>
                <field id="fileid" type="BaseType.U_FILEID" required="false" multi="false" array="false" longname="文件ID号" ref="BaseDict.Comm.fileid"/>
                <field id="mdcode" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="MD5码" ref="BaseDict.Comm.mdcode"/>
                <field id="canshulb" type="IoApFileBatchType.BizParmInfo" required="false" multi="true" array="false" longname="业务参数列表"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="btflmd" type="BaseType.U_CHAR04" required="false" multi="false" array="false" longname="批量文件类型" ref="BaseDict.Comm.btflmd"/>
                <field id="entrno" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="委托号" ref="BaseDict.Comm.entrno"/>
                <field id="succfg" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="成功标志" ref="BaseDict.Comm.succfg"/>
                <field id="errocd" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="错误代码" ref="BaseDict.Comm.errocd"/>
                <field id="errotx" type="BaseType.U_LONGDS" required="false" multi="false" array="false" longname="错误信息" ref="BaseDict.Comm.errotx"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="QueryFileBatchStatus" name="queryFileBatchStatus" longname="文件批量状态查询">
        <interface>
            <input packMode="false">
                <field id="entrno" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="委托号" ref="BaseDict.Comm.entrno"/>
                <field id="btflmd" type="BaseType.U_CHAR04" required="false" multi="false" array="false" longname="批量文件类型" ref="BaseDict.Comm.btflmd"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="btflmd" type="BaseType.U_CHAR04" required="false" multi="false" array="false" longname="批量文件类型" ref="BaseDict.Comm.btflmd"/>
                <field id="entrno" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="委托号" ref="BaseDict.Comm.entrno"/>
                <field id="flbtst" type="ApBaseEnumType.E_FLBTST" required="false" multi="false" array="false" longname="文件批量状态" ref="BaseDict.Comm.flbtst"/>
                <field id="totanm" type="BaseType.U_LONGIN" required="false" multi="false" array="false" longname="总笔数" ref="BaseDict.Comm.totanm"/>
                <field id="succnm" type="BaseType.U_LONGIN" required="false" multi="false" array="false" longname="成功笔数" ref="BaseDict.Comm.succnm"/>
                <field id="failnm" type="BaseType.U_LONGIN" required="false" multi="false" array="false" longname="失败笔数" ref="BaseDict.Comm.failnm"/>
                <field id="fileid" type="BaseType.U_FILEID" required="false" multi="false" array="false" longname="文件ID号" ref="BaseDict.Comm.fileid"/>
                <field id="mdcode" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="MD5码" ref="BaseDict.Comm.mdcode"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
          <service id="FileInteractiveReqByServer" name="FileInteractiveReqByServer" longname="文件交互请求封装服务">
        <interface>
            <input packMode="true">
                <field id="source" type="PbEnumType2.E_SYSNAME" required="false" multi="false" array="false" longname="原系统标识"/>
                <field id="target" type="PbEnumType2.E_SYSNAME" required="false" multi="false" array="false" longname="目标系统标识"/>
                <field id="dataid" type="BaseType2.U_CHAR50" required="false" multi="false" array="false" longname="数据类型"/>
                <field id="busseq" type="BaseType2.U_CHAR50" required="false" multi="false" array="false" longname="业务流水"/>
                <field id="acctdt" type="BaseType2.U_CHAR32" required="false" multi="false" array="false" longname="会计日期"/>
                <field id="status" type="PbEnumType2.E_FLJHST" required="false" multi="false" array="false" longname="文件交互状态"/>
                <field id="descri" type="BaseType2.U_CHAR80" required="false" multi="false" array="false" longname="文件交互结果描述"/>
                <field id="fileList" type="ApDefineType.PssFilePlat" required="false" multi="true" array="false" longname="数据集合"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="busseq" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="业务流水"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="updateKapbPlwjxxb" name="updateKapbPlwjxxb" longname="变更文件批量信息表信息">
        <interface>
            <input packMode="false">
                <field id="wplxxb" type="SysFileTables.kapb_wjplxxb" required="false" multi="false" array="false" longname="文件信息表"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
