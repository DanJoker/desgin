<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<schema id="SysSynFileTables" package="cn.sunline.ltts.busi.aplt.tables" longname="系统批量数据迁移" classgen="normal" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <table id="AppDataMove" name="app_data_move" longname="数据迁移参数表" virtual="false" abstract="false">
        <fields>
            <field id="dacdcn" longname="导出DCN" type="BaseType.U_DCNNO" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="daocbm" longname="导出表名" type="BaseType.U_CHAR32" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="dardcn" longname="导入DCN" type="BaseType.U_DCNNO" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="daorbm" longname="导入表名" type="BaseType.U_CHAR32" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="namsql" longname="导出SQL语句" type="BaseType.U_CHAR400" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="synctp" longname="同步方式" type="BaseEnumType.E_SYNCTP" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="unikey" longname="主键" type="BaseType.U_CHAR100" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="selway" longname="主键查询更新方法名" type="BaseType.U_CHAR32" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="dbdcnf" longname="删除所依据dcn字段" type="BaseType.U_CHAR10" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
        </fields>
        <ddls/>
        <odbindexes>
            <index id="odb1" type="unique" fields="dacdcn daocbm dardcn daorbm" operate="selectOne deleteOne updateOne" longname="根据导入、导出DCN节点" cache="none"/>
            <index id="odb2" type="index" fields="dardcn" operate="selectAll" longname="查询需要导入的数据" cache="none"/>
        </odbindexes>
        <indexes>
            <index id="app_data_move_uk1" type="unique" fields="dacdcn daocbm dardcn daorbm">
                <ddls/>
            </index>
        </indexes>
    </table>
    <table id="ApbFileStat" name="apb_file_stat" longname="文件生成状态表" virtual="false" abstract="false">
        <fields>
            <field id="trandt" longname="交易日期" type="BaseType.U_DATE08" ref="BaseDict.Comm.trandt" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="prcscd" longname="交易码" type="BaseType.U_PRCSCD" ref="BaseDict.Comm.prcscd" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="filena" longname="文件名称" type="BaseType.U_CHAR100" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="filest" longname="文件类型" type="BaseEnumType.E_FILEST" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
        </fields>
        <ddls/>
        <odbindexes>
            <index id="odb1" type="unique" fields="trandt filena" operate="selectOne deleteOne updateOne"/>
        </odbindexes>
        <indexes>
            <index id="apb_file_stat_uk1" type="unique" fields="trandt filena">
                <ddls/>
            </index>
        </indexes>
    </table>
</schema>
