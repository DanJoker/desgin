<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="IoPbInRaSelSvc" kind="business" longname="利息税率相关服务" package="cn.sunline.ltts.busi.iobus.servicetype.pb" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <description><![CDATA[利息税率相关服务]]></description>
    <service id="IoPbInRaSel" name="pbinrasel" longname="利率税率查询">
        <interface>
            <input packMode="true">
                <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="当前页数"/>
                <field id="pagesize" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="每页记录数"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="result" type="IoPbintrratsel.Pbintrratsel" required="false" multi="true" array="false" longname="利息税税率查询"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoProRateSel" name="proratesel" longname="查询利率代码名称">
        <interface>
            <input packMode="false">
                <field id="intrcd" type="BaseType.U_INRTNO" required="false" multi="false" array="false" longname="利率编号" ref="DpDict.Prod.intrcd"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="intrna" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="利率名称" ref="PbDict.Intr.intrna"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoInttxRate" name="inttxRate" longname="利息税率查询">
        <interface>
            <input packMode="false">
                <field id="intxcd" type="BaseType.U_TXRTNO" required="false" multi="false" array="false" longname="税率代码"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="taxrat" type="BaseType.U_TAXRAT" required="false" multi="false" array="false" longname="税率"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoGetNearTxTate" name="getNearTxTate" longname="获取离起息日最近的生效税率">
        <interface>
            <input packMode="false">
                <field id="intxcd" type="BaseType.U_TXRTNO" required="false" multi="false" array="false" longname="税率代码"/>
                <field id="efctdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="生效日期"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="taxrat" type="BaseType.U_TAXRAT" required="false" multi="false" array="false" longname="税率"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoGetTxrtEfctdtNearStdate" name="getTxrtEfctdtNearStdate" longname="获取离起始日期最近的税率生效日期">
        <interface>
            <input packMode="false">
                <field id="begndt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="起始日期" ref="DpDict.Depr.begndt"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="efctdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="生效日期" ref="BaseDict.Comm.efctdt"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoGetIntxrtList" name="getIntxrtList" longname="获取生效日期之后的税率列表">
        <interface>
            <input packMode="false">
                <field id="efctdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="生效日期" ref="BaseDict.Comm.efctdt"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="txlist" type="IoPbintrratsel.Pbintrratsel" required="false" multi="true" array="false" longname="利息税率信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoPbAddIntxrtInfo" name="addKupInrxRate" longname="利率税率添加">
        <interface>
            <input packMode="false">
                <field id="intxIn" type="IoIntrComplexType.IoAddKupIntxIn" required="false" multi="false" array="false" longname="利息税信息"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="IoPbDelIntxrtInfo" name="delKupInrxRate" longname="删除利率税率">
        <interface>
            <input packMode="false">
                <field id="intxcd" type="BaseType.U_TXRTNO" required="false" multi="false" array="false" longname="税率编号"/>
                <field id="efctdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="生效日期" ref="BaseDict.Comm.efctdt"/>
                <field id="corpno" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="法人代码" desc="法人代码" ref="ApDict.Aplt.corpno"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="IoPbSelIntrTaxList" name="selIntrTaxList" longname="通过交易日期查询税率代码列表">
        <interface>
            <input packMode="false">
                <field id="corpno" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="法人代码" desc="法人代码" ref="ApDict.Aplt.corpno"/>
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="DpDict.Acct.trandt"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="txlist" type="IoPbintrratsel.IntrTaxList" required="false" multi="true" array="false" longname="利息税率信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="IoPbCntIntxcodSelPb" name="cntIntxcodSelPb" longname="根据税率代码查询税率代码定义总条数">
        <interface>
            <input packMode="false">
                <field id="intxcd" type="BaseType.U_TXRTNO" required="false" multi="false" array="false" longname="税率编号"/>
                <field id="corpno" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="法人代码" desc="法人代码" ref="ApDict.Aplt.corpno"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="taxcnt" type="long" required="false" multi="false" array="false" longname="税率代码总条数"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
