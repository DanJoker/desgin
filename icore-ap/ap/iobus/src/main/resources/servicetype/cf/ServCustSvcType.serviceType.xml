<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="ServCustSvcType" kind="business" longname="客户账户信息服务" package="cn.sunline.ltts.busi.iobus.servicetype.cf" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <description><![CDATA[客户账户公用服务]]></description>
    <service id="QryCustInfo" name="qryCustInfo" longname="客户信息查询">
        <interface>
            <input packMode="false">
                <field id="custno" type="BaseType.U_CUSTNO" required="false" multi="false" array="false" longname="客户号" ref="BaseDict.Comm.custno"/>
                <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型"/>
                <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="custfo" type="IoCuCustComplexType.CustInfo" required="false" multi="false" array="false" longname="地址"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="UptCustInfo" name="uptCustInfo" longname="客户信息修改">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号"/>
                <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型"/>
                <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码"/>
                <field id="emails" type="BaseType.U_CHAR200" required="false" multi="false" array="false" longname="邮箱"/>
                <field id="teleno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="电话"/>
                <field id="postcd" type="BaseType.U_CHAR10" required="false" multi="false" array="false" longname="邮编"/>
                <field id="addres" type="BaseType.U_CHAR400" required="false" multi="false" array="false" longname="地址"/>
                <field id="agisln" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="证件是否长期有效"/>
                <field id="agstdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="证件生效日期"/>
                <field id="agendt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="证件失效日期"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="QryCustSuppInfo" name="qryCustSuppInfo" longname="客户附加信息查询">
        <interface>
            <input packMode="true">
                <field id="custno" type="BaseType.U_CUSTNO" required="false" multi="false" array="false" longname="客户号" ref="BaseDict.Comm.custno"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="suppfo" type="IoCuTable.IoCifCustSupp" required="false" multi="false" array="false" longname="客户附加信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="UptCustSuppInfo" name="uptCustSuppInfo" longname="客户附加信息修改">
        <interface>
            <input packMode="true">
                <field id="suppfo" type="IoCuTable.IoCifCustSupp" required="false" multi="false" array="false" longname="客户附加信息"/>
            </input>
            <output asParm="true" packMode="true"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="IoUptCorpCustInfo" name="uptCorpCustInfo" longname="对公客户信息修改">
        <interface>
            <input packMode="false">
                <field id="corpif" type="IoCuTable.IoCuCifCorp" required="false" multi="false" array="false" longname="对公客户信息"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="IoUptCorpCustSuppInfo" name="uptCorpCustSuppInfo" longname="对公客户附加信息修改">
        <interface>
            <input packMode="false">
                <field id="suppif" type="IoCuTable.IoCifCorpSupp" required="false" multi="false" array="false" longname="对公附加信息"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
