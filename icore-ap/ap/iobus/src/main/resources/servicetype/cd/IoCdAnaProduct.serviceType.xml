<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoCdAnaProduct" kind="business" longname="协议存款服务" package="cn.sunline.ltts.busi.iobus.servicetype.cd" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="AnaProductForOpen" name="anaProductForOpen" longname="存款产品分析规则">
        <interface>
            <input packMode="true">
                <field id="dcmttp" type="BaseType.U_DCMTTP" required="false" multi="false" array="false" longname="凭证类型"/>
                <field id="dcmtno" type="BaseType.U_DCMTNO" required="false" multi="false" array="false" longname="凭证号码"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="AnaProductForOpac" name="anaProductForOpac" longname="存款产品分析规则">
        <interface>
            <input packMode="true">
                <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="账号"/>
                <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种"/>
                <field id="debttp" type="BaseEnumType.E_DEBTTP" required="false" multi="false" array="false" longname="储种"/>
                <field id="spectp" type="CtEnumType.E_SPECTP" required="false" multi="false" array="false" longname="账户性质"/>
                <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品代码"/>
                <field id="custtp" type="BaseEnumType.E_CUSTTP" required="false" multi="false" array="false" longname="客户类型"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="lvdefn" type="DpEnumType.E_LVDEFN" required="false" multi="false" array="false" longname="分层标准定义"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="AnalyseProductForSave" name="analyseProductForSave" longname="存款子交易分析产品">
        <interface>
            <input packMode="true">
                <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="账号"/>
                <field id="dpacno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="子账号"/>
                <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种"/>
                <field id="csextg" type="BaseEnumType.E_CSEXTG" required="false" multi="false" array="false" longname="钞汇标志"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="dpacno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="子户号"/>
                <field id="opentg" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="开立子户标志" desc="1-开立子户 0-不开立子户"/>
                <field id="spectp" type="CtEnumType.E_SPECTP" required="false" multi="false" array="false" longname="账户类型"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="updateKnsTranInfo" name="updateKnsTranInfo" longname="更新交易资金信息">
        <interface>
            <input packMode="true">
                <field id="crcycd" type="string" required="false" multi="false" array="false" longname="币种"/>
                <field id="tranam" type="amount" required="false" multi="false" array="false" longname="交易金额"/>
                <field id="acctno" type="string" required="false" multi="false" array="false" longname="账号"/>
                <field id="dpacno" type="string" required="false" multi="false" array="false" longname="子账号"/>
                <field id="acctna" type="string" required="false" multi="false" array="false" longname="账户户名"/>
                <field id="acctbr" type="string" required="false" multi="false" array="false" longname="账户部门"/>
                <field id="dcmttp" type="string" required="false" multi="false" array="false" longname="凭证类型"/>
                <field id="dcmtno" type="string" required="false" multi="false" array="false" longname="凭证号码"/>
                <field id="cheqtp" type="string" required="false" multi="false" array="false" longname="支票凭证类型"/>
                <field id="cheqno" type="string" required="false" multi="false" array="false" longname="支票凭证号码"/>
                <field id="invodt" type="string" required="false" multi="false" array="false" longname="出票日期"/>
                <field id="toacct" type="string" required="false" multi="false" array="false" longname="对方账号"/>
                <field id="todpac" type="string" required="false" multi="false" array="false" longname="对方子账号"/>
                <field id="toacna" type="string" required="false" multi="false" array="false" longname="对方账户名称"/>
                <field id="toacbr" type="string" required="false" multi="false" array="false" longname="对方开户部门"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="registerTranExtd" name="registerTranExtd" longname="登记交易扩展信息">
        <interface>
            <input packMode="true">
                <field id="agnttg" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否代理"/>
                <field id="agidtp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="代理人证件类型"/>
                <field id="agidno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="代理人证件号码"/>
                <field id="agcuna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="代理人名称"/>
                <field id="desctx" type="BaseType.U_CHAR200" required="false" multi="false" array="false" longname="备注"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="checkPayasqMoney" name="checkPayasqMoney" longname="待销帐全额销账检查">
        <interface>
            <input packMode="true">
                <field id="payasq" type="BaseType.U_CHAR20" required="false" multi="false" array="false" longname="待销帐流水"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
