<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="IoAccountSvcType" kind="business" longname="记账服务" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <description><![CDATA[登记会计流水
进行财务平衡性检查]]></description>
    
    <service id="IoSelKnsProdClerInfo" name="selKnsProdClerInfo" longname="查询产品信息">
        <interface>
            <input packMode="false">
                <field id="corpno" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="法人代码" ref="ApDict.Aplt.corpno"/>
                <field id="prodcd" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="产品代码" ref="PbDict.Dcmt.prodcd"/>
                <field id="eventp" type="BaseType.U_CHAR08" required="false" multi="false" array="false" longname="交易信息"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="knsProdCler" type="IoAccountComplexType.IoKnsProdClerInfo" required="false" multi="true" array="false" longname="产品核算信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoCheckStatusBeforeAccount" name="checkStatusBeforeAccount" longname="记账前账户状态检查">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="客户账号" ref="BaseDict.Comm.custac"/>
                <field id="amntcd" type="BaseEnumType.E_AMNTCD" required="false" multi="false" array="false" longname="借贷标志" ref="BaseDict.Comm.amntcd"/>
                <field id="iscodr" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否控制借冻"/>
                <field id="clactp" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="销户标识"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoQueryKnlBill" name="QueryKnlBill" longname="查询账户交易明细信息">
        <interface>
            <input packMode="false">
                <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="账号"/>
                <field id="bgindt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="起始交易日期"/>
                <field id="endddt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="终止交易日期"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="knlBillInfo" type="IoDpQueryComplexType.IoKnlBillInfo" required="false" multi="true" array="false" longname="账户发生明细信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    
    <service id="IoQueryKnsTranByUserid" name="qryKnsTranByUserid" longname="分页查询零售节点交易流水信息(外调)">
        <interface>
            <input packMode="true">
                <field id="cdcnno" type="BaseType.U_DCNNO" required="false" multi="false" array="false" longname="DCN编号" ref="BaseDict.Comm.cdcnno"/>
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="BaseDict.Comm.trandt"/>
                <field id="tranus" type="BaseType.U_USERID" required="false" multi="false" array="false" longname="交易柜员" ref="BaseDict.Comm.tranus"/>
                <field id="pageno" type="long" required="false" multi="false" array="false" longname="页数"/>
                <field id="pagsiz" type="long" required="false" multi="false" array="false" longname="页容量"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="tranList" type="IoAccountComplexType.DpKnsTranTemp" required="false" multi="true" array="false" longname="会计流水临时表"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    
    <service id="IoQueryAdmKnsTran" name="queryAdmKnsTran" longname="查询ADM节点交易流水">
        <interface>
            <input packMode="false">
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="BaseDict.Comm.trandt"/>
                <field id="tranus" type="BaseType.U_USERID" required="false" multi="false" array="false" longname="交易柜员" ref="BaseDict.Comm.tranus"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="tranList" type="IoAccountComplexType.DpKnsTranTemp" required="false" multi="true" array="false" longname="会计流水临时表"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    
</serviceType>
