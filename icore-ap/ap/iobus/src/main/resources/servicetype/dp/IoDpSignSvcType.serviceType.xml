<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoDpSignSvcType" kind="business" longname="产品签约相关" package="cn.sunline.ltts.busi.iobus.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="SignIn" name="signin" longname="产品签约">
        <interface>
            <input packMode="true">
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号"/>
                <field id="custac" type="BaseType.U_CUSTAC" required="false" multi="false" array="false" longname="电子账号"/>
                <field id="signtp" type="BaseEnumType.E_SIGNTP" required="false" multi="false" array="false" longname="签约类型"/>
                <field id="signdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="签约日期"/>
                <field id="invadt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="失效日期"/>
                <field id="remamt" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="留存金额"/>
                <field id="limamt" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="签约额度"/>
                <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品编号"/>
                <field id="prodtx" type="BaseType.U_CLOB" required="false" multi="false" array="false" longname="产品属性"/>
                <field id="autofg" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否自动签约"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="signno" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="签约编号"/>
                <field id="status" type="BaseEnumType.E_FAIL__" required="false" multi="false" array="false" longname="成功失败状态"/>
                <field id="errotx" type="OdBaseType.U_ERROTX" required="false" multi="false" array="false" longname="错误信息"/>
                <field id="prodna" type="LnBaseType.U_PRODNA" required="false" multi="false" array="false" longname="产品名称"/>
            </output>
            <property packMode="true"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="SignOt" name="signot" longname="产品解约">
        <interface>
            <input packMode="true">
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号"/>
                <field id="signtp" type="BaseEnumType.E_SIGNTP" required="false" multi="false" array="false" longname="签约类型"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="signno" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="签约编号"/>
                <field id="status" type="BaseEnumType.E_FAIL__" required="false" multi="false" array="false" longname="成功失败状态"/>
                <field id="errotx" type="OdBaseType.U_ERROTX" required="false" multi="false" array="false" longname="错误信息"/>
            </output>
            <property packMode="true"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="QySign" name="qysign" longname="产品签约解约查询">
        <interface>
            <input packMode="true">
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号"/>
                <field id="signtp" type="BaseEnumType.E_SIGNTP" required="false" multi="false" array="false" longname="签约类型"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="signlist" type="IoDpSignComplexType.IoKnaSignJdctInfo" required="false" multi="true" array="false" longname="签约解约列表"/>
            </output>
            <property packMode="true"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
