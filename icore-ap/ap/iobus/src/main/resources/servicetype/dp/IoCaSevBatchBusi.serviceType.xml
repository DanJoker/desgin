<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoCaSevBatchBusi" kind="business" longname="电子账户批量开户服务定义" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="SelDCNCardnoCount" name="selDCNCardnoCount" longname="查询节点账户数量">
        <interface>
            <input packMode="true">
                <field id="cdcnno" type="BaseType.U_DCNNO" required="false" multi="false" array="false" longname="DCN编号" ref="BaseDict.Comm.cdcnno"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="cadcnt" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="账户数量"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="QryOpenrtResult" name="qryOpenrtResult" longname="查询批量开户结果">
        <interface>
            <input packMode="true">
                <field id="btchno" type="ApBaseType.U_PILJYLSH" required="false" multi="false" array="false" longname="批次号"/>
                <field id="openrt" type="CaEnumType.E_OPENRT" required="false" multi="false" array="false" longname="开户结果"/>
                <field id="pageno" type="long" required="false" multi="false" array="false" longname="起始页数"/>
                <field id="pgsize" type="long" required="false" multi="false" array="false" longname="每页条数"/>
                <field id="cdcnno" type="BaseType.U_DCNNO" required="false" multi="false" array="false" longname="DCN编号" ref="BaseDict.Comm.cdcnno"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="count" type="long" required="false" multi="false" array="false" longname="总笔数"/>
                <field id="btoprtInfoList" type="ServEacctType.EacctBtoprtInfo" required="false" multi="true" array="false" longname="批量开户结果集"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="SelDCNCustByIdtfno" name="selDCNCustByIdtfno" longname="检查身份证号码是否在其他节点开户成功">
        <interface>
            <input packMode="true">
                <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号" ref="BaseDict.Comm.idtfno"/>
                <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型" ref="BaseDict.Comm.idtftp"/>
                <field id="custna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="客户名称" ref="BaseDict.Comm.custna"/>
                <field id="custno" type="BaseType.U_CUSTNO" required="false" multi="false" array="false" longname="客户号" ref="BaseDict.Comm.custno"/>
                <field id="cacttp" type="BaseType.U_CACTTP" required="false" multi="false" array="false" longname="客户账号类型" ref="DpDict.Prod.cacttp"/>
                <field id="cdcnno" type="BaseType.U_DCNNO" required="false" multi="false" array="false" longname="DCN编号" ref="BaseDict.Comm.cdcnno"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="iscust" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否开立客户"/>
                <field id="iscard" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否开立账户"/>
                <field id="custno" type="BaseType.U_CUSTNO" required="false" multi="false" array="false" longname="客户号" ref="BaseDict.Comm.custno"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="UpdKnbOpbt" name="updKnbOpbt" longname="批量开户信息维护">
        <interface>
            <input packMode="false">
                <field id="cplKnbOpbt" type="ServEacctType.EacctBtoprtNewAndUpd" required="false" multi="false" array="false" longname="批量开户登记簿复合类型"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="cplotbt" type="ServEacctType.EacctBtoprtNewAndUpd" required="false" multi="false" array="false" longname="批量开户登记簿复合类型"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="SelKnbOpbtList" name="selKnbOpbtList" longname="分页查询批量开户登记簿">
        <interface>
            <input packMode="true">
                <field id="btchno" type="ApBaseType.U_PILJYLSH" required="false" multi="false" array="false" longname="批量交易批次号"/>
                <field id="acctdt" type="PbBaseType.U_DATE08" required="false" multi="false" array="false" longname="日期"/>
                <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码"/>
                <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="当前页数"/>
                <field id="pagesz" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="当前页容量"/>
                <field id="cdcnno" type="BaseType.U_DCNNO" required="false" multi="false" array="false" longname="DCN编号" ref="BaseDict.Comm.cdcnno"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="cplKnbOpbt" type="ServEacctType.EacctBtoprtNewAndUpd" required="false" multi="true" array="false" longname="批量开户登记簿"/>
                <field id="counts" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="总记录数"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="InsKnbOpbt" name="insKnbOpbt" longname="新增批量开户信息">
        <interface>
            <input packMode="false">
                <field id="cplKnbOpbt" type="ServEacctType.EacctBtoprtNewAndUpd" required="false" multi="false" array="false" longname="批量开户登记簿信息"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="cplOptInfo" type="ServEacctType.EacctBtoprtInsRtnInfo" required="false" multi="false" array="false" longname="批量开户新增结果"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="DelKnbOpbt" name="delKnbOpbt" longname="删除单条批量开户信息">
        <interface>
            <input packMode="false">
                <field id="btchno" type="ApBaseType.U_PILJYLSH" required="false" multi="false" array="false" longname="批量交易批次号"/>
                <field id="btchnm" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="批量交易序号"/>
                <field id="acctdt" type="PbBaseType.U_DATE08" required="false" multi="false" array="false" longname="日期"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="SelKnbOpbt" name="selKnbOpbt" longname="查询单条批量开户">
        <interface>
            <input packMode="false">
                <field id="btchno" type="ApBaseType.U_PILJYLSH" required="false" multi="false" array="false" longname="批量交易批次号"/>
                <field id="btchnm" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="批量交易序号"/>
                <field id="acctdt" type="PbBaseType.U_DATE08" required="false" multi="false" array="false" longname="日期"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="cplKnbOpbt" type="ServEacctType.EacctBtoprtNewAndUpd" required="false" multi="false" array="false" longname="批量开户登记簿"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
