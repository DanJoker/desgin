            <output asParm="false" packMode="false">
                <field id="onlnbl" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="账户余额"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoCheckCustIdtfno" name="checkCustIdtfno" longname="检查一卡通客户证件信息">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="客户账号" ref="PbDict.Charge.custac"/>
                <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码" ref="PbDict.Cash.idtfno"/>
                <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件种类" ref="PbDict.Cash.idtftp"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoCardFixCancel" name="IoCardFixCancel" longname="销定期子户">
