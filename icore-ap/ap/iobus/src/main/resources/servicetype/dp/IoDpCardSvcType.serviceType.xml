            <input packMode="false">
                <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="账号" ref="PbDict.Charge.acctno"/>
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.custac"/>
                <field id="pddpfg" type="DpEnumType.E_FCFLAG" required="false" multi="false" array="false" longname="定活标志"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="onlnbl" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="账户余额"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoFxacCanclChk" name="fxacCanclChk" longname="定期销户检查">
