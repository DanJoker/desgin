            <output asParm="true" packMode="true">
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账户"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoCheckCardclInfo" name="checkCardclInfo" longname="检查一卡通销户">
        <interface>
            <input packMode="true">
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号" ref="DpDict.Acct.cardno"/>
                <field id="tranam" type="PbBaseType.U_TRANAM" required="false" multi="false" array="false" longname="交易金额" ref="PbDict.Charge.tranam"/>
