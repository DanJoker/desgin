<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoDpCardSvcType" kind="business" longname="一卡通相关服务" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="IoCardEndInt" name="cardEndInt" longname="一卡通结息">
