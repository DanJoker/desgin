<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoCaSevGenBindCard" kind="business" longname="电子账户绑卡服务" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[电子账户绑卡服务]]></description>
    <service id="IoCaSevGenBindCar" name="addBindCard" longname="电子账户绑卡服务">
        <interface>
            <input packMode="false">
                <field id="caBindCard" type="IoCaTypeGenBindCard.IoCaSrvGenBindCard" required="false" multi="false" array="false" longname="电子账户绑卡服务接口"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoCaRemoveBindCar" name="removeBindCar" longname="解除电子账户绑卡服务">
        <interface>
            <input packMode="false">
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号" ref="DpDict.Acct.cardno"/>
                <field id="odopac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="原绑定账户"/>
                <field id="cdopac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="绑定账户" ref="CaDict.Eacct.cdopac"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoCaSelBinDCar" name="selBindCarInfo" longname="查询电子账户绑卡">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号"/>
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号"/>
                <field id="cardtp" type="CaEnumType.E_BACATP" required="false" multi="false" array="false" longname="绑定账户类型" ref="CaDict.Eacct.cardtp"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="bindCardInfos" type="IoCaTypeGenBindCard.IoCaBindCardInfo" required="false" multi="true" array="false" longname="查询电子账户绑卡信息输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoCaSevGenBindCarList" name="addBindCarList" longname="电子账户绑定多张卡服务">
        <interface>
            <input packMode="false">
                <field id="lstBindcaInfo" type="IoCaTypeGenBindCard.IoCaBindCardInfo" required="false" multi="true" array="false" longname="绑定账户信息"/>
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.custac"/>
                <field id="accatp" type="BaseEnumType.E_ACCATP" required="false" multi="false" array="false" longname="账户分类" ref="DpDict.Prod.accatp"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoCaSelBindByCard" name="selBindByCard" longname="使用电子账号和卡号查询绑定卡信息">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号ID"/>
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="外部卡号"/>
                <field id="status" type="BaseEnumType.E_DPACST" required="false" multi="false" array="false" longname="状态"/>
                <field id="isable" type="boolean" required="false" multi="false" array="false" longname="是否抛出异常"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="cacd" type="IoCaTable.IoCaKnaCacd" required="false" multi="false" array="false" longname="绑定卡信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoCaCardAlterReg" name="regCardAlter" longname="绑定账户信息修改登记">
        <interface>
            <input packMode="false">
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="电子账号"/>
                <field id="odopac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="原绑定账号"/>
                <field id="cdopac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="新绑定账号"/>
                <field id="mantwy" type="CaEnumType.E_MANTWY" required="false" multi="false" array="false" longname="维护方式" ref="CaDict.Eacct.mantwy"/>
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.custac"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
</serviceType>
