                <field id="sbactp" type="BaseType.U_CHAR08" required="false" multi="false" array="false" longname="子账户类型"/>
                <field id="pytltp" type="QtEnumType.E_PYTLTP" required="false" multi="false" array="false" longname="支付工具"/>
                <field id="limtst" type="QtEnumType.E_LIMTST" required="false" multi="false" array="false" longname="限额状态"/>
