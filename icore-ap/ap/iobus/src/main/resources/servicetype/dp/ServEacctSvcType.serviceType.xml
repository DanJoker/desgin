                <field id="prodls" type="ServEacctType.EacctInfoBal" required="false" multi="true" array="false" longname="产品收益率列表"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="QryTransferDetail" name="qryTransferDetail" longname="查询电子账号转账交易明细">
        <interface>
            <input packMode="true">
                <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.ecctno"/>
                <field id="corpno" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="法人代码" ref="ApDict.Aplt.corpno"/>
                <field id="pageno" type="long" required="false" multi="false" array="false" longname="起始页数"/>
                <field id="recdct" type="long" required="false" multi="false" array="false" longname="每页条数"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="infos" type="ServEacctType.EacctTransDetailInfoOut" required="false" multi="true" array="false" longname="电子账户转账明细输出"/>
                <field id="count" type="long" required="false" multi="false" array="false" longname="总记录数"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="QryCustnaAndCardno" name="qryCustnaAndCardno" longname="根据电子账号查询卡号和户名">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="客户账号" ref="BaseDict.Comm.custac"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="custnacardno" type="ServEacctType.EacctCustnaCardno" required="false" multi="false" array="false" longname="户名和卡号"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
