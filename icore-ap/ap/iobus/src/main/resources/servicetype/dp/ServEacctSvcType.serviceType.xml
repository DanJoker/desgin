<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="ServEacctSvcType" kind="business" longname="电子账户信息服务" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <description><![CDATA[电子账户公用服务]]></description>
    <service id="QryEacctInfo" name="qryEacctInfo" longname="电子账户信息查询">
        <interface>
            <input packMode="false">
                <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号"/>
                <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种" ref="BaseDict.Comm.crcycd"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="cplxEacctInfo" type="ServEacctType.EacctInfoOut" required="false" multi="false" array="false" longname="电子账户信息输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="QryEacctTransDtlInfo" name="qryEacctTransDtlInfo" longname="电子账户交易明细查询">
        <interface>
            <input packMode="false">
                <field id="transDtlInfoIn" type="ServEacctType.EacctTransDtlInfoIn" required="false" multi="false" array="false" longname="交易明细输入"/>
