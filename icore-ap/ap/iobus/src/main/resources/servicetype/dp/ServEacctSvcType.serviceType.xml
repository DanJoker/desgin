            <output asParm="false" packMode="false">
                <field id="transDtlInfoOut" type="ServEacctType.EacctTransDtlInfoOut" required="false" multi="false" array="false" longname="电子账户交易明细查询输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="QryEacctPlanIn" name="qryEacctPlanIn" longname="靠档预期收益查询">
        <interface>
            <input packMode="false">
                <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.ecctno"/>
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="DpDict.Acct.trandt"/>
                <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种" ref="BaseDict.Comm.crcycd"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.ecctno"/>
                <field id="planin" type="BaseType.U_TOTAIN" required="false" multi="false" array="false" longname="计提利息" ref="DpDict.Prod.planin"/>
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="DpDict.Acct.trandt"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="UptEacctInfo" name="uptEacctInfo" longname="电子账户信息修改">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.custac"/>
                <field id="acctst" type="BaseEnumType.E_ACCTST" required="false" multi="false" array="false" longname="电子账户状态" ref="CaDict.Eacct.acctst"/>
                <field id="emails" type="BaseType.U_CHAR200" required="false" multi="false" array="false" longname="邮件" ref="DpDict.Acct.emails"/>
                <field id="addres" type="BaseType.U_CHAR200" required="false" multi="false" array="false" longname="地址" ref="PbDict.Brch.addres"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
