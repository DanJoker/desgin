            <input packMode="false">
                <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型"/>
                <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码"/>
                <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="账户所属机构" ref="DpDict.Acct.brchno"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="counts" type="int" required="false" multi="false" array="false" longname="数量"/>
                <field id="globct" type="int" required="false" multi="false" array="false" longname="全功能账户数量"/>
                <field id="finact" type="int" required="false" multi="false" array="false" longname="理财户数量"/>
                <field id="waltct" type="int" required="false" multi="false" array="false" longname="钱包户数量"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
