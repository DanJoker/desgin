    <service id="QryTransDetail" name="qryTransDetail" longname="电子账户交易明细查询">
        <interface>
            <input packMode="false">
                <field id="transDtlInfoIn" type="ServEacctType.EacctTransDtlInfoIn" required="false" multi="false" array="false" longname="交易明细输入"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="transDtlInfoOut" type="ServEacctType.EacctTransDtlInfoOut" required="false" multi="false" array="false" longname="电子账户交易明细查询输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
