                <field id="limttp" type="BaseType.U_CHAR08" required="false" multi="false" array="false" longname="额度类型"/>
                <field id="limtkd" type="QtEnumType.E_LIMTKD" required="false" multi="false" array="false" longname="额度种类"/>
                <field id="servtp" type="BaseType.U_CHAR08" required="false" multi="false" array="false" longname="渠道"/>
