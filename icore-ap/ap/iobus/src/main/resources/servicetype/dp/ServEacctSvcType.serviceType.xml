        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="客户账号"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="custid" type="BaseType.U_CUSTID" required="false" multi="false" array="false" longname="用户ID"/>
            </output>
