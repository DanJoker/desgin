<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoQtAuthLimit" kind="business" longname="认证方式限额" package="cn.sunline.ltts.busi.iobus.servicetype.qt" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[认证方式限额]]></description>
    <service id="AuthLimitAdd" name="authlimitadd" longname="认证方式限额新增">
        <interface>
            <input packMode="true">
                <field id="infos" type="IoQtAuthLimitInfo.IoAuthMothIn" required="false" multi="true" array="false" longname="认证方式限额输入"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="AuthLimitUpd" name="authlimitupd" longname="认证方式限额修改">
        <interface>
            <input packMode="true">
                <field id="infos" type="IoQtAuthLimitInfo.IoAuthMothIn" required="false" multi="true" array="false" longname="认证方式限额修改输入"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="AuthLimitDel" name="authlimitdel" longname="认证方式限额删除">
        <interface>
            <input packMode="true">
                <field id="infos" type="IoQtAuthLimitInfo.IoAuthMothDel" required="false" multi="true" array="false" longname="认证方式限额删除输入"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="AuthLimitSel" name="authlimitsel" longname="认证方式限额查询">
        <interface>
            <input packMode="true">
                <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="机构号" ref="PbDict.Brch.brchno"/>
                <field id="acctrt" type="BaseEnumType.E_ACCTROUTTYPE" required="false" multi="false" array="false" longname="账号路由类型" ref="BaseDict.Comm.acctrt"/>
                <field id="limttp" type="BaseType.U_CHAR02" required="false" multi="false" array="false" longname="额度类型"/>
                <field id="limtkd" type="QtEnumType.E_LIMTKD" required="false" multi="false" array="false" longname="额度种类" ref="QtDict.Qt.limtkd"/>
                <field id="servtp" type="BaseType.U_CHAR04" required="false" multi="false" array="false" longname="渠道"/>
                <field id="risklv" type="QtEnumType.E_RISKLV" required="false" multi="false" array="false" longname="风险承受等级" ref="QtDict.Qt.risklv"/>
                <field id="authtp" type="BaseType.U_CHAR100" required="false" multi="false" array="false" longname="认证方式"/>
                <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="当前页数"/>
                <field id="pgsize" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="每页记录数"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="outinfo" type="IoQtAuthLimitInfo.IoAuthMothup" required="false" multi="true" array="false" longname="认证方式限额查询输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
</serviceType>
