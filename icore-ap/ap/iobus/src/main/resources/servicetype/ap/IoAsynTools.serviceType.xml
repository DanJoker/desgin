<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoAsynTools" kind="business" longname="异步服务查询" package="cn.sunline.ltts.busi.iobus.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[账号信息查询]]></description>
    <service id="getRecdno" name="getRecdno" longname="获取记录次序号">
        <interface>
            <input packMode="false">
                <field id="cdcnno" type="BaseType.U_CHAR08" required="true" multi="false" array="false" longname="DCN编号"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="kmpDcndyInfo" type="IoApPubComplex.KmpDcndyInfo" required="false" multi="false" array="false" longname="记录次序号"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="lstKmpDcndy" name="lstKmpDcndy" longname="获取DCN定义表全部数据">
        <interface>
            <input packMode="true"/>
            <output asParm="false" packMode="false">
                <field id="lstdcn" type="IoApPubComplex.KmpDcndyInfo" required="false" multi="true" array="false" longname="DCN定义表"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="selDCNCount" name="selDCNCount" longname="查询零售节点数量">
        <interface>
            <input packMode="true"/>
            <output asParm="false" packMode="false">
                <field id="counts" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="数量"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
