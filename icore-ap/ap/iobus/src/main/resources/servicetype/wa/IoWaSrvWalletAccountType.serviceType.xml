                <field id="reltst" type="WaEnumType.E_RELTST" required="false" multi="false" array="false" longname="亲情钱包状态"/>
                <field id="clostx" type="WaEnumType.E_CLOSTX" required="false" multi="false" array="false" longname="关闭原因"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoWaSubQuota" name="ioWaSubQuota" longname="亲情钱包额度扣减">
        <interface>
            <input packMode="false">
                <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="负债账号" ref="WaDict.WaAcc.acctno"/>
                <field id="tranam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="交易金额" ref="BaseDict.Comm.tranam"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
