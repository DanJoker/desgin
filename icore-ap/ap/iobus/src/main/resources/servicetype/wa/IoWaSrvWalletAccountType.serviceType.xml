<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoWaSrvWalletAccountType" kind="business" longname="亲情钱包账户服务" package="cn.sunline.ltts.busi.iobus.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
