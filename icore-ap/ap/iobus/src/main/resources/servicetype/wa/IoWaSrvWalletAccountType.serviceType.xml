    <service id="IoWaQryWactInfo" name="qryWactInfo" longname="查询可用亲情钱包信息">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CUSTAC" required="false" multi="false" array="false" longname="电子账号" ref="FnDict.Fnfc.custac"/>
