            </input>
            <output asParm="false" packMode="false">
                <field id="reltid" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="亲情关系ID"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoWaAddWaQuot" name="addWaQuot" longname="新增亲情钱包额度信息">
        <interface>
            <input packMode="false">
                <field id="cplWaQuot" type="WaAcctType.fquotInfo" required="false" multi="false" array="false" longname="亲情钱包额度输入信息"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
