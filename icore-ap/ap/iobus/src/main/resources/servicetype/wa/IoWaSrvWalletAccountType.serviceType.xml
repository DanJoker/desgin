            <input packMode="false">
                <field id="reltid" type="BaseType.U_RELTID" required="false" multi="false" array="false" longname="亲情关系ID" ref="WaDict.WaAcc.reltid"/>
                <field id="upflag" type="WaEnumType.E_UPFLAG" required="false" multi="false" array="false" longname="合约变更方式" ref="WaDict.WaAcc.upflag"/>
                <field id="closfg" type="WaEnumType.E_CLOSFG" required="false" multi="false" array="false" longname="关闭人标志" ref="WaDict.WaAcc.closfg"/>
            </input>
            <output asParm="true" packMode="false"/>
