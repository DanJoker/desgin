        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CUSTAC" required="false" multi="false" array="false" longname="创建人电子账号ID"/>
                <field id="elacct" type="BaseType.U_CUSTAC" required="false" multi="false" array="false" longname="关联人电子账号"/>
                <field id="start" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="页码"/>
                <field id="count" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="页容量"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="waDetailOuts" type="WaAcctType.QryWaDetailOut" required="false" multi="true" array="false" longname="亲情钱包明细信息"/>
            </output>
