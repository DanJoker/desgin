        <interface>
            <input packMode="false">
                <field id="reltid" alias="亲情关系ID" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="亲情关系ID"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="waDescOut" type="WaAcctType.WaDescOut" required="false" multi="false" array="false" longname="亲情钱包详情信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
