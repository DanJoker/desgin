            </input>
            <output asParm="false" packMode="false">
                <field id="tblKnpBusiScen" type="IoInTable.IoKnpBusiScen" required="false" multi="false" array="false" longname="内部户产品参数配置表"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
