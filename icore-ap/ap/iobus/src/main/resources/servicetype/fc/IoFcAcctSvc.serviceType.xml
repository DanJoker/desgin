        <interface>
            <input packMode="false">
                <field id="lstrdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="上次交易日期" ref="DpDict.Acct.lstrdt"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="genSubjInfoFile" name="genSubjInfoFile" longname="生成标的状态文件">
        <interface>
            <input packMode="false">
                <field id="lstrdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="上次交易日期" ref="DpDict.Acct.lstrdt"/>
            </input>
            <output asParm="true" packMode="false"/>
