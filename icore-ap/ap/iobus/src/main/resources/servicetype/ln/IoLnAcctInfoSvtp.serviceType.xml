                <field id="initam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="贷款余额"/>
                <field id="lendbl" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="可用额度"/>
                <field id="borrer" type="BaseType.U_CHAR12" required="false" multi="false" array="false" longname="借款人"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
