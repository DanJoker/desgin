                <field id="repytp" type="LnEnumType.E_REPYTP" required="false" multi="false" array="false" longname="还款种类" ref="LnDict.LNF.repytp"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="LnExempt" name="LnExempt" longname="委托贷款利息豁免">
        <interface>
            <input packMode="false">
                <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="贷款账号"/>
                <field id="exinam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="免除本金"/>
                <field id="exintr" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="免除利息"/>
                <field id="tomony" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="豁免总额"/>
                <field id="isdfcd" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="委托账户是否跨节点"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="lnExemptOutput" type="LnAcctComplexType.LnExemptOutput" required="false" multi="false" array="false" longname="贷款豁免输出"/>
