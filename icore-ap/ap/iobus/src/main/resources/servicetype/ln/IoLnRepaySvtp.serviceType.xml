                <field id="rpinpt" type="LnRepayComplexType.LnRepayInput" required="false" multi="false" array="false" longname="还款输入"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="rpotpt" type="LnRepayComplexType.LnRepayOutput" required="false" multi="false" array="false" longname="还款输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
