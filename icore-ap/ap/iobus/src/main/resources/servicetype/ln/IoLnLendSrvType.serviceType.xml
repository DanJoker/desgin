<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoLnLendSrvType" kind="business" longname="贷款发放服务" package="cn.sunline.ltts.busi.iobus.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[2019.1.8
1、完全剔除电子账号关联，由交易层自行关联
2、借据关联客户号
3、增加分布式路由注册]]></description>
    <service id="LnLendGrantDeal" name="prcLendGrantDeal" longname="贷款放款处理服务">
        <interface>
            <input packMode="false">
                <field id="cplRouteIn" type="IoRouteType.ProcRouteInput" required="false" multi="false" array="false" longname="消费交易路由分发输入"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="cplLendOut" type="LnLendComplexType.LnLendGrantOut" required="false" multi="false" array="false" longname="贷款放款输出对象"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="LnChangeSchdDeal" name="changeSchdDeal" longname="自定义还款计划变更">
        <interface>
            <input packMode="false">
                <field id="tmpls" type="LnLendComplexType.LnChangeSchdDealIn" required="true" multi="true" array="false" longname="自定义还款计划变更输入"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="LnLendCustomDeal" name="prcLendCustomDeal" longname="自定义计划贷款放款处理服务">
        <interface>
            <input packMode="false">
                <field id="cplLnCtomIn" type="LnLendComplexType.LnLendCustomIn" required="false" multi="false" array="false" longname="自定义计划贷款放款输入"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="cplLnCtomOut" type="LnLendComplexType.LnLendCustomOut" required="false" multi="false" array="false" longname="自定义计划贷款放款输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="LnLendLnbCtom" name="prcLnbCtom" longname="大麦白条预约">
        <interface>
            <input packMode="false">
                <field id="lncfno" type="LnBaseType.U_LNCFNO" required="false" multi="false" array="false" longname="贷款借据号" ref="LnDict.LNF.lncfno"/>
                <field id="isfxfg" type="LnEnumType.E_YES___" required="false" multi="false" array="false" longname="免息期到期是否分期"/>
                <field id="schdtp" type="LnEnumType.E_SCHDTP" required="false" multi="false" array="false" longname="还款方式" ref="LnDict.LNF.schdtp"/>
                <field id="relntm" type="LnBaseType.U_DEADLI" required="false" multi="false" array="false" longname="还款期限" ref="LnDict.LNF.relntm"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="LnLendCloseInfo" name="lendCloseInfo" longname="大麦白条销户服务">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="LnDict.LNF.custac"/>
                <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品代码" ref="LnDict.LNF.prodcd"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="LnCtomRefundDeal" name="prcLnCtomRefundDeal" longname="自定义还款计划贷款退款服务">
        <interface>
            <input packMode="false">
                <field id="cplRefdin" type="LnLendComplexType.LnCtomRefundIn" required="false" multi="false" array="false" longname="自定义贷款退款输入"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="cplRefdou" type="LnLendComplexType.LnCtomRefundOut" required="false" multi="false" array="false" longname="自定义贷款退款输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
</serviceType>
