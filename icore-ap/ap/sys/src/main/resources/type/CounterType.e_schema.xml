        <enumeration id="FUNCTION" value="F" longname="功能"/>
        <enumeration id="SENCE" value="S" longname="场景"/>
    </restrictionType>
    <restrictionType id="E_BRANCHSTAS" longname="机构开机状态" base="string" maxLength="1">
        <enumeration id="OPENNING" value="O" longname="开机"/>
        <enumeration id="CLOSEING" value="C" longname="关机"/>
    </restrictionType>
    <restrictionType id="E_OVERSHORTSTAS" longname="长短款状态" base="string" maxLength="1">
        <enumeration id="REGISTER" value="R" longname="登记"/>
        <enumeration id="DONE" value="D" longname="已处理"/>
        <enumeration id="PART" value="P" longname="部分处理"/>
    </restrictionType>
   	<restrictionType id="E_CASHOPERATEIND" longname="现金操作标志" base="string" maxLength="1">
        <enumeration id="CALLOUT" value="C" longname="调出"/>
        <enumeration id="PAIYD" value="P" longname="入账"/>
    </restrictionType>
    <restrictionType id="E_EGENTIND" longname="代收代付标志" base="string" maxLength="1">
        <enumeration id="CHARGING" value="C" longname="代收"/>
        <enumeration id="PAYING" value="P" longname="代付"/>
    </restrictionType>
    <restrictionType id="E_OSOPERATEIND" longname="长短款处理标志" base="string" maxLength="1">
        <enumeration id="REGISTER" value="0" longname="登记"/>
        <enumeration id="CASH" value="1" longname="现金"/>
        <enumeration id="OFF_BUSI" value="2" longname="转营业外"/>
        <enumeration id="CUST_ACCT" value="3" longname="客户账号"/>
        <enumeration id="INSIDE_ACCT" value="4" longname="资金账号"/>
    </restrictionType>
    <restrictionType id="E_FUNDSOURCE" longname="资金来源" base="string" maxLength="1">
        <enumeration id="CASH" value="1" longname="现金"/>
        <enumeration id="TRANS" value="2" longname="转账"/>
        <enumeration id="SUSPENSE" value="3" longname="挂账"/>
        <enumeration id="WRITEOFF" value="4" longname="销账"/>
        <enumeration id="CHEQUE" value="5" longname="支票"/>
        <enumeration id="CONTINUESUSPENSE" value="6" longname="续挂"/>
    </restrictionType>
</schema>