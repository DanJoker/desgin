<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<schema id="CounterType" package="cn.sunline.ltts.sys.type" longname="enum-type(counter system)" classgen="normal" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 	<restrictionType id="E_TELLERSTAS" longname="柜员状态" base="string" maxLength="1">
        <enumeration id="NORMAL" value="N" longname="正常"/>
        <enumeration id="CLOSED" value="C" longname="关闭"/>
    </restrictionType>
    <restrictionType id="E_PASSWDSTAS" longname="密码状态" base="string" maxLength="1">
        <enumeration id="NORMAL" value="N" longname="正常"/>
        <enumeration id="FROZEN" value="F" longname="禁止"/>
        <enumeration id="OVERDUE" value="O" longname="过期"/>
    </restrictionType>
    <restrictionType id="E_TEXNTYLE" longname="业务受理方式" base="string" maxLength="1">
        <enumeration id="DIRECT" value="A" longname="直通"/>
        <enumeration id="AUTHORIZATION" value="B" longname="授权"/>
        <enumeration id="WORKFLOW" value="C" longname="工作流"/>
    </restrictionType>  
    <restrictionType id="E_ROLESYL" longname="资源类型" base="string" maxLength="1">
        <enumeration id="MENU_ID" value="A" longname="菜单id"/>
        <enumeration id="MENU_GROUP" value="B" longname="菜单组"/>
        <enumeration id="VIEW_BRANCH_DATA" value="C" longname="分支数据视图"/>
        <enumeration id="TRANSATION_CODE" value="D" longname="柜面交易码"/>
    </restrictionType>
    <restrictionType id="E_OPERIND" longname="操作分级标志" base="string" maxLength="1">
        <enumeration id="NOT_LEVEL" value="A" longname="不标准"/>
        <enumeration id="LEVEL" value="B" longname="标准"/>
    </restrictionType> 
    <restrictionType id="E_OPERTYPE" longname="授权作业类型" base="string" maxLength="1">
        <enumeration id="OPERATE" value="A" longname="开放"/>
        <enumeration id="AUTHORIZATION" value="B" longname="授权"/>
    </restrictionType>   
    <restrictionType id="E_RECSTYLE" longname="任务签收方式" base="string" maxLength="1">
        <enumeration id="DESIGNATED" value="A" longname="指定签收"/>
        <enumeration id="FIRST" value="B" longname="先到签收"/>
    </restrictionType>
    <restrictionType id="E_BRCONFINE" longname="角色所在机构范围" base="string" maxLength="1">
        <enumeration id="CURRENT_BRANCH" value="A" longname="当前机构"/>
        <enumeration id="UPPER_BRANCH" value="B" longname="上级机构"/>
        <enumeration id="DESIGNATED_BRANCH" value="C" longname="指定机构"/>
        <enumeration id="DESIGNATED_FILE_OF_BRANCH" value="D" longname="指定字段对应机构"/>
    </restrictionType>   
    <restrictionType id="E_LINETYPE" longname="工作路线种类" base="string" maxLength="1">
        <enumeration id="COMMITE" value="C" longname="指派"/>
        <enumeration id="BACK_UP" value="B" longname="退回"/>
        <enumeration id="REFUSE" value="R" longname="拒绝"/>
    </restrictionType> 
    <restrictionType id="E_OPENSTYLE" longname="页面打开方式" base="string" maxLength="1">
        <enumeration id="SHEET" value="1" longname="指派"/>
        <enumeration id="RESPONSE" value="2" longname="响应窗口"/>
    </restrictionType>
    <restrictionType id="E_PAGEDEAL" longname="页面整体处理" base="string" maxLength="1">
        <enumeration id="STAY_SAME" value="A" longname="保持一致"/>
        <enumeration id="DISABLED" value="B" longname="缺省"/>
    </restrictionType>
    <restrictionType id="E_FLOWSTATE" longname="流程处理状态" base="string" maxLength="1">
        <enumeration id="NOT_CLOSED" value="N" longname="未关闭"/>
        <enumeration id="CLOSED" value="C" longname="关闭"/>
    </restrictionType>
    <restrictionType id="E_JOBSTATE" longname="任务处理状态" base="string" maxLength="1">
        <enumeration id="TODO" value="0" longname="待办"/>
        <enumeration id="DONE" value="1" longname="完成"/>
    </restrictionType>
    <restrictionType id="E_MEANSCOMU" longname="通讯方式" base="string" maxLength="1">
        <enumeration id="TCP" value="1" longname="tcp"/>
        <enumeration id="HTTP" value="2" longname="http"/>
    </restrictionType>
    <restrictionType id="E_LOGSTAS" longname="柜员登录状态" base="string" maxLength="1">
        <enumeration id="ONLINE" value="1" longname="在线"/>
        <enumeration id="OFFLINE" value="0" longname="下线"/>
    </restrictionType>
