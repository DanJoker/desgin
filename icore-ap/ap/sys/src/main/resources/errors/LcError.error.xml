<?xml version="1.0" encoding="UTF-8"?>
<errorConf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="LcError" longname="卡贷通错误码" package="cn.sunline.ltts.busi.sys.errors" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
