<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- [Template id=异步消息配置 longname=异步消息配置 ] -->
<amsgConfig iscInstanceId="${系统实例ID|2001}"  iscClient="${是否发布|false}" iscServer="${是否订阅|false}" 
    registIscLogSucess="${是否登记发送成功日志|false}" registIscLogFail="${是否登记发送失败日志|true}"
	reqEnvSchemaName="${公共请求对象|TrxAmsgEnvs.Request}" 
	rspEnvSchemaName="${公共响应对象|TrxAmsgEnvs.Response}">
	
	<!-- [Template id=订阅配置 longname=订阅配置 ] -->
	<amsgServerConfig name="${名称|AmsgServer}" minPoolSize="${最小连接数|30}" 
		maxPoolSize="${最大连接数|60}" longname="${中文名称|内部服务器}" 
		keepAliveTime="${空闲线程保持时间|300}" queueSize="${客户端等待处理最大队列数|500}" 
		queueIndex="${接收队列编号|0}" />
	<!-- [End] -->
</amsgConfig>
<!-- [End] -->