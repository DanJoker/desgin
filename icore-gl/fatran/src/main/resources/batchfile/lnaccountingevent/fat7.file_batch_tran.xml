<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fat7" longname="读取及处理场景事件流水文件" kind="read" package="cn.sunline.edsp.busi.gl.fatran.batchfile.lnaccountingevent" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <description><![CDATA[文件批形式]]></description>
