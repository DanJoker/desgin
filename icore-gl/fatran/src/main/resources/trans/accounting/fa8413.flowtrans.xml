<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="fa8413" longname="套账明细删除" kind="M" package="cn.sunline.ltts.busi.fatran.trans.accounting" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.fatran.trans.accounting.intf">
        <input packMode="true">
            <field id="set_account_no" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="套号" ref="GlDict.A.set_account_no"/>
            <field id="data_sort" type="GlBaseType.U_LONG" required="false" multi="false" array="false" longname="数据序号" ref="GlDict.A.data_sort"/>
            <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false"  longname="数据版本号" ref="BaseDict.Comm.recdver"/>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvFaAccounting.delSetAccountDetail" id="SrvFaAccounting.delSetAccountDetail" longname="套账明细删除">
            <in_mappings by_interface="true">
                <mapping src="data_sort" dest="detailDelIn.data_sort" by_interface="true" on_top="true"/>
                <mapping src="recdver" dest="detailDelIn.recdver" by_interface="true" on_top="true"/>
                <mapping src="set_account_no" dest="detailDelIn.set_account_no" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
        <!-- service serviceName="SrvApTrxn.registerApsTransaction" id="SrvApTrxn.registerApsTransaction" longname="登记流水">
            <in_mappings by_interface="true"/>
            <out_mappings by_interface="true"/>
        </service-->
    </flow>
</flowtran>
