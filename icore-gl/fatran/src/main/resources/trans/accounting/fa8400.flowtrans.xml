<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="fa8400" longname="套账申请" kind="M" package="cn.sunline.ltts.gl.fatran.trans.accounting" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.gl.fatran.trans.accounting.intf">
        <input packMode="true">
            <field id="subject_accounting_ind" type="GlEnumType.E_YESORNO" required="false" multi="false" array="false" longname="科目记账标志" ref="GlDict.A.subject_accounting_ind"/>
            <field id="remark" type="GlBaseType.U_SHORTDESC" required="false" multi="false" array="false" longname="备注" ref="GlDict.A.remark"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="sys_no" type="GlBaseType.U_SYSNO" required="true" multi="false" array="false" longname="系统编号" ref="GlDict.A.sys_no"/>
            <field id="set_account_no" type="BaseType.U_SEQUNO" required="true" multi="false" array="false" longname="套号" ref="GlDict.A.set_account_no"/>
            <field id="set_account_type" type="GlBusinessType.E_SETACCOUNTTYPE" required="true" multi="false" array="false" longname="套帐类别" ref="GlDict.A.set_account_type"/>
            <field id="subject_accounting_ind" type="GlEnumType.E_YESORNO" required="false" multi="false" array="false" longname="科目记账标志" ref="GlDict.A.subject_accounting_ind"/>
            <field id="trxn_date" type="GlBaseType.U_DATE" required="true" multi="false" array="false" longname="交易日期" ref="GlDict.A.trxn_date"/>
            <field id="trxn_seq" type="BaseType.U_SEQUNO" required="true" multi="false" array="false" longname="交易流水" ref="GlDict.A.trxn_seq"/>
            <field id="trxn_branch" type="BaseType.U_BRCHNO" required="true" multi="false" array="false" longname="交易机构" ref="GlDict.A.trxn_branch"/>
            <field id="trxn_teller" type="GlBaseType.U_USERID" required="true" multi="false" array="false" longname="交易柜员" ref="GlDict.A.trxn_teller"/>
            <field id="set_account_status" type="GlBusinessType.E_SETACCOUNTSTATE" required="true" multi="false" array="false" longname="套账状态" ref="GlDict.A.set_account_status"/>
            <field id="posting_date" type="GlBaseType.U_DATE" required="false" multi="false" array="false" longname="入账日期" ref="GlDict.A.posting_date"/>
            <field id="posting_seq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="入账流水" ref="GlDict.A.posting_seq"/>
            <field id="posting_branch" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="入账机构" ref="GlDict.A.posting_branch"/>
            <field id="posting_teller" type="GlBaseType.U_USERID" required="false" multi="false" array="false" longname="入账柜员" ref="GlDict.A.posting_teller"/>
            <field id="remark" type="GlBaseType.U_SHORTDESC" required="false" multi="false" array="false" longname="备注" ref="GlDict.A.remark"/>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvFaAccounting.applySetAccountsNo" id="SrvFaAccounting.applySetAccountsNo" longname="申请套号">
            <in_mappings by_interface="true">
                <mapping src="remark" dest="remark" by_interface="true" on_top="true"/>
                <mapping src="subject_accounting_ind" dest="subject_accounting_ind" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="mainOut.posting_branch" dest="posting_branch" by_interface="true" on_top="true"/>
                <mapping src="mainOut.posting_date" dest="posting_date" by_interface="true" on_top="true"/>
                <mapping src="mainOut.posting_seq" dest="posting_seq" by_interface="true" on_top="true"/>
                <mapping src="mainOut.posting_teller" dest="posting_teller" by_interface="true" on_top="true"/>
                <mapping src="mainOut.remark" dest="remark" by_interface="true" on_top="true"/>
                <mapping src="mainOut.set_account_no" dest="set_account_no" by_interface="true" on_top="true"/>
                <mapping src="mainOut.set_account_status" dest="set_account_status" by_interface="true" on_top="true"/>
                <mapping src="mainOut.set_account_type" dest="set_account_type" by_interface="true" on_top="true"/>
                <mapping src="mainOut.subject_accounting_ind" dest="subject_accounting_ind" by_interface="true" on_top="true"/>
                <mapping src="mainOut.sys_no" dest="sys_no" by_interface="true" on_top="true"/>
                <mapping src="mainOut.trxn_branch" dest="trxn_branch" by_interface="true" on_top="true"/>
                <mapping src="mainOut.trxn_date" dest="trxn_date" by_interface="true" on_top="true"/>
                <mapping src="mainOut.trxn_seq" dest="trxn_seq" by_interface="true" on_top="true"/>
                <mapping src="mainOut.trxn_teller" dest="trxn_teller" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
        <!-- service serviceName="SrvApTrxn.registerApsTransaction" id="SrvApTrxn.registerApsTransaction" longname="登记流水">
            <in_mappings by_interface="true">
                <mapping src="remark" dest="trxn_info.customer_remark" by_interface="true" on_top="true"/>
                <mapping src="remark" dest="trxn_info.trxn_remark" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service-->
    </flow>
</flowtran>
