<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fa8130" longname="系统定义查询" kind="1" package="cn.sunline.ltts.busi.fatran.trans.sysdefine" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.ltts.busi.fatran.trans.sysdefine.intf">
        <input packMode="true">
            <field id="sys_no" type="GlBaseType.U_SYSNO" required="false" multi="false" array="false" longname="系统编号" ref="GlDict.A.sys_no"/>
            <field id="sys_name" type="GlBaseType.U_SHORTDESC" required="false" multi="false" array="false" longname="系统名称" ref="GlDict.A.sys_name"/>
            <field id="system_service_status" type="GlBusinessType.E_SYSTEMSERVICESTATUS" required="false" multi="false" array="false" longname="系统服务状态" ref="GlDict.A.system_service_status"/>
            <field id="system_date" type="GlBaseType.U_DATE" required="false" multi="false" array="false" longname="系统日期" ref="GlDict.A.system_date"/>
            <field id="busi_seq_format" type="GlBusinessType.E_BUSISEQFORMAT" required="false" multi="false" array="false" longname="业务流水组织形式" ref="GlDict.A.busi_seq_format"/>
            <field id="ledger_clearing_gl_code" type="GlBaseType.U_SUBJECTNO" required="false" multi="false" array="false" longname="与总账往来科目" ref="GlDict.A.ledger_clearing_gl_code"/>
            <field id="error_gl_code" type="GlBaseType.U_SUBJECTNO" required="false" multi="false" array="false" longname="差错补偿科目" ref="GlDict.A.error_gl_code"/>
            <field id="remark" type="GlBaseType.U_SHORTDESC" required="false" multi="false" array="false" longname="备注" ref="GlDict.A.remark"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="list01" required="false" multi="true" longname="系统定义查询" array="false">
                <field id="sys_no" type="GlBaseType.U_SYSNO" required="true" multi="false" array="false" longname="系统编号" ref="GlDict.A.sys_no"/>
                <field id="sys_name" type="GlBaseType.U_SHORTDESC" required="true" multi="false" array="false" longname="系统名称" ref="GlDict.A.sys_name"/>
                <field id="system_service_status" type="GlBusinessType.E_SYSTEMSERVICESTATUS" required="true" multi="false" array="false" longname="系统服务状态" ref="GlDict.A.system_service_status"/>
                <field id="system_date" type="GlBaseType.U_DATE" required="true" multi="false" array="false" longname="系统日期" ref="GlDict.A.system_date"/>
                <field id="busi_seq_format" type="GlBusinessType.E_BUSISEQFORMAT" required="true" multi="false" array="false" longname="业务流水组织形式" ref="GlDict.A.busi_seq_format"/>
                <field id="ledger_clearing_gl_code" type="GlBaseType.U_SUBJECTNO" required="true" multi="false" array="false" longname="与总账往来科目" ref="GlDict.A.ledger_clearing_gl_code"/>
                <field id="error_gl_code" type="GlBaseType.U_SUBJECTNO" required="true" multi="false" array="false" longname="差错补偿科目" ref="GlDict.A.error_gl_code"/>
                <field id="remark" type="GlBaseType.U_SHORTDESC" required="true" multi="false" array="false" longname="备注" ref="GlDict.A.remark"/>
                <field id="corpno" type="BaseType.U_CORPNO" required="true" multi="false" array="false" longname="法人代码" ref="ApDict.Aplt.corpno"/>
                <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false" longname="数据版本号" ref="BaseDict.Comm.recdver"/>
            </fields>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="SrvFaSysDefine.getSysDefine" id="SrvFaSysDefine.getSysDefine" longname="系统定义列表查询">
            <in_mappings by_interface="true">
                <mapping src="busi_seq_format" dest="queryIn.busi_seq_format" by_interface="true" on_top="true"/>
                <mapping src="ledger_clearing_gl_code" dest="queryIn.ledger_clearing_gl_code" by_interface="true" on_top="true"/>
                <mapping src="error_gl_code" dest="queryIn.error_gl_code" by_interface="true" on_top="true"/>
                <mapping src="remark" dest="queryIn.remark" by_interface="true" on_top="true"/>
                <mapping src="sys_name" dest="queryIn.sys_name" by_interface="true" on_top="true"/>
                <mapping src="sys_no" dest="queryIn.sys_no" by_interface="true" on_top="true"/>
                <mapping src="system_date" dest="queryIn.system_date" by_interface="true" on_top="true"/>
                <mapping src="system_service_status" dest="queryIn.system_service_status" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="queryOut" dest="list01" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
