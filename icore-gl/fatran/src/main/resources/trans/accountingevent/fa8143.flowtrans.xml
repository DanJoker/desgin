<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fa8143" longname="会计核算事件信息列表查询" kind="1" package="cn.sunline.ltts.busi.fatran.trans.accountingevent" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.ltts.busi.fatran.trans.intf">
        <input packMode="true">
            <field id="sys_no" type="GlBaseType.U_SYSNO" required="false" multi="false" array="false" longname="系统编号" ref="GlDict.A.sys_no"/>
            <field id="accounting_subject" type="GlBaseType.U_ACCOUNTINGSUBJECT" required="false" multi="false" array="false" longname="会计主体" ref="GlDict.A.accounting_subject"/>
            <field id="accounting_alias" type="GlBaseType.U_BYTE32" required="false" multi="false" array="false" longname="核算别名" ref="GlDict.A.accounting_alias"/>
            <field id="remark" type="GlBaseType.U_SHORTDESC" required="false" multi="false" array="false" longname="备注" ref="GlDict.A.remark"/>
            <field id="bal_attributes" type="GlBaseType.U_BALPROPERTY" required="false" multi="false" array="false" longname="余额属性" ref="GlDict.A.bal_attributes"/>
            <field id="gl_code" type="GlBaseType.U_SUBJECTNO" required="false" multi="false" array="false" longname="科目号" ref="GlDict.A.gl_code"/>
            <field id="offset_gl_code" type="GlBaseType.U_SUBJECTNO" required="false" multi="false" array="false" longname="对方科目" ref="GlDict.A.offset_gl_code"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="list01" required="false" multi="true" longname="查询输出" array="false">
                <field id="sys_no" type="GlBaseType.U_SYSNO" required="true" multi="false" array="false" longname="系统编号" ref="GlDict.A.sys_no"/>
                <field id="accounting_subject" type="GlBaseType.U_ACCOUNTINGSUBJECT" required="true" multi="false" array="false" longname="会计主体" ref="GlDict.A.accounting_subject"/>
                <field id="accounting_alias" type="GlBaseType.U_BYTE32" required="true" multi="false" array="false" longname="核算别名" ref="GlDict.A.accounting_alias"/>
                <field id="remark" type="GlBaseType.U_SHORTDESC" required="false" multi="false" array="false" longname="备注" ref="GlDict.A.remark"/>
                <field id="bal_attributes" type="GlBaseType.U_BALPROPERTY" required="true" multi="false" array="false" longname="余额属性" ref="GlDict.A.bal_attributes"/>
                <field id="gl_code" type="GlBaseType.U_SUBJECTNO" required="true" multi="false" array="false" longname="科目号" ref="GlDict.A.gl_code"/>
                <field id="offset_gl_code" type="GlBaseType.U_SUBJECTNO" required="true" multi="false" array="false" longname="对方科目" ref="GlDict.A.offset_gl_code"/>
                <field id="corpno" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="法人代码" ref="ApDict.Aplt.corpno"/>
                <field id="recdver" type="BaseType.U_RECDVER" required="false" multi="false" array="false" longname="记录版本号" ref="BaseDict.Comm.recdver"/>
            </fields>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvFaAccountingEvent.queryEvent" id="SrvFaAccountingEvent.queryEvent" longname="查询列表">
            <in_mappings by_interface="true">
                <mapping src="accounting_alias" dest="queryIn.accounting_alias" by_interface="true" on_top="true"/>
                <mapping src="accounting_subject" dest="queryIn.accounting_subject" by_interface="true" on_top="true"/>
                <mapping src="bal_attributes" dest="queryIn.bal_attributes" by_interface="true" on_top="true"/>
                <mapping src="offset_gl_code" dest="queryIn.offset_gl_code" by_interface="true" on_top="true"/>
                <mapping src="remark" dest="queryIn.remark" by_interface="true" on_top="true"/>
                <mapping src="gl_code" dest="queryIn.gl_code" by_interface="true" on_top="true"/>
                <mapping src="sys_no" dest="queryIn.sys_no" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="queryOut" dest="list01" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
