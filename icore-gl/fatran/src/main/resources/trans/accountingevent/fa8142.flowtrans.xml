<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fa8142" longname="会计核算事件删除" kind="2" package="cn.sunline.ltts.busi.fatran.trans.accountingevent" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.ltts.busi.fatran.trans.intf">
        <input packMode="true">
            <field id="sys_no" type="GlBaseType.U_SYSNO" required="true" multi="false" array="false" longname="系统编号" ref="GlDict.A.sys_no"/>
            <field id="accounting_alias" type="GlBaseType.U_BYTE32" required="true" multi="false" array="false" longname="核算别名" ref="GlDict.A.accounting_alias"/>
            <field id="bal_attributes" type="GlBaseType.U_BALPROPERTY" required="true" multi="false" array="false" longname="余额属性" ref="GlDict.A.bal_attributes"/>
            <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false" longname="数据版本号" ref="BaseDict.Comm.recdver"/>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvFaAccountingEvent.delAccountingEvent" id="SrvFaAccountingEvent.delAccountingEvent" longname="删除一条信息">
            <in_mappings by_interface="true">
                <mapping src="accounting_alias" dest="accounting_alias" by_interface="true" on_top="true"/>
                <mapping src="bal_attributes" dest="bal_attributes" by_interface="true" on_top="true"/>
                <mapping src="recdver" dest="recdver" by_interface="true" on_top="true"/>
                <mapping src="sys_no" dest="sys_no" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
