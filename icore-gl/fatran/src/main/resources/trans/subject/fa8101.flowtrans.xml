<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fa8101" longname="科目信息建立" kind="2" package="cn.sunline.ltts.busi.fatran.trans.subject" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.ltts.busi.fatran.trans.subject.intf">
        <input packMode="true">
            <fields id="list01" required="false" multi="true" longname="增加信息输入" array="false">
                <field id="gl_code" type="GlBaseType.U_SUBJECTNO" required="true" multi="false" array="false" longname="科目号" ref="GlDict.A.gl_code"/>
                <field id="gl_code_desc" type="GlBaseType.U_SHORTDESC" required="true" multi="false" array="false" longname="科目名称" ref="GlDict.A.gl_code_desc"/>
                <field id="upper_lvl_gl_code" type="GlBaseType.U_SUBJECTNO" required="false" multi="false" array="false" longname="上级科目" ref="GlDict.A.upper_lvl_gl_code"/>
                <field id="gl_code_type" type="GlBusinessType.E_SUBJECTTYPE" required="true" multi="false" array="false" longname="科目类别" ref="GlDict.A.gl_code_type"/>
                <field id="gl_code_level" type="GlBaseType.U_LONG" required="true" multi="false" array="false" longname="科目级别" ref="GlDict.A.gl_code_level"/>
                <field id="end_gl_code_ind" type="GlEnumType.E_YESORNO" required="true" multi="false" array="false" longname="末层科目标志" ref="GlDict.A.end_gl_code_ind"/>
                <field id="bal_prop" type="GlBusinessType.E_BALPROP" required="true" multi="false" array="false" longname="余额性质" ref="GlDict.A.bal_prop"/>
                <field id="on_bal_sheet_ind" type="GlEnumType.E_YESORNO" required="true" multi="false" array="false" longname="表内标志" ref="GlDict.A.on_bal_sheet_ind"/>
                <field id="simple_list_display_ind" type="GlEnumType.E_YESORNO" required="true" multi="false" array="false" longname="简表展示标识" ref="GlDict.A.simple_list_display_ind"/>
                <field id="bal_check_ind" type="GlEnumType.E_YESORNO" required="true" multi="false" array="false" longname="余额检查标志" ref="GlDict.A.bal_check_ind"/>
                <field id="debit_manual_allow" type="GlEnumType.E_YESORNO" required="true" multi="false" array="false" longname="借方手工记账许可" ref="GlDict.A.debit_manual_allow"/>
                <field id="credit_manual_allow" type="GlEnumType.E_YESORNO" required="true" multi="false" array="false" longname="贷方手工记账许可" ref="GlDict.A.credit_manual_allow"/>
                <field id="manual_open_acct_mode" type="GlBusinessType.E_MANUALOPENACCTMODE" required="false" multi="false" array="false" longname="手工开户受理模式" ref="GlDict.A.manual_open_acct_mode"/>
                <field id="allow_accounting_sys" type="GlBaseType.U_SYSNO" required="false" multi="false" array="false" longname="允许记账的系统" ref="GlDict.A.allow_accounting_sys"/>
                <field id="valid_ind" type="GlEnumType.E_YESORNO" required="true" multi="false" array="false" longname="有效标志" ref="GlDict.A.valid_ind"/>
                <field id="opp_open_ind" type="GlEnumType.E_YESORNO" required="false" multi="false" array="false" longname="对开标志" ref="GlDict.A.opp_open_ind"/>
                <field id="offset_gl_code" type="GlBaseType.U_SUBJECTNO" required="false" multi="false" array="false" longname="对方科目" ref="GlDict.A.offset_gl_code"/>
                <field id="opp_open_way" type="GlBusinessType.E_OPPOPENWAY" required="false" multi="false" array="false" longname="对开账户识别方式" ref="GlDict.A.opp_open_way"/>
            </fields>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="SrvFaAccoutingSubject.addSubjectList" id="SrvFaAccoutingSubject.addSubjectList" longname="新增一条或多条科目信息">
            <in_mappings by_interface="true">
                <mapping src="list01" dest="addIn" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
