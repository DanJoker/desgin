                <field id="effect_date" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="生效日期" ref="GlDict.A.effect_date"/>
                <field id="invalid_date" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="失效日期" ref="GlDict.A.invalid_date"/>
                <field id="tran_user" type="BaseType.U_USERID" required="false" multi="false" array="false" longname="新增柜员"/>
                <field id="auth_user" type="BaseType.U_USERID" required="false" multi="false" array="false" longname="新增授权柜员(串)"/>
                <field id="last_tran_user" type="BaseType.U_USERID" required="false" multi="false" array="false" longname="最后修改柜员"/>
