                <field id="scene_dimension_thirteen" type="GlBaseType.U_BYTE40" required="false" multi="false" array="false" longname="场景维度13"/>
                <field id="scene_dimension_fourteen" type="GlBaseType.U_BYTE40" required="false" multi="false" array="false" longname="场景维度14"/>
