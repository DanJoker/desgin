                <field id="scene_code" type="GlBaseType.U_SCENE_CODE" required="false" multi="false" array="false" longname="场景编号" ref="GlDict.A.scene_code"/>
                <field id="scene_name" type="GlBaseType.U_SCENE_NAME" required="false" multi="false" array="false" longname="场景名称" ref="GlDict.A.scene_name"/>
                <field id="product_code" type="GlBaseType.U_PRODUCT_CODE" required="false" multi="false" array="false" longname="产品编号" ref="GlDict.A.product_code"/>
                <field id="product_name" type="GlBaseType.U_PRODUCT_NAME" required="false" multi="false" array="false" longname="产品名称" ref="GlDict.A.product_name"/>
                <field id="bal_type" type="GlBaseType.U_BAL_TYPE" required="false" multi="false" array="false" longname="金额类别" ref="GlDict.B.bal_type"/>
                <field id="loan_term" type="GlBaseType.U_LOAN_TERM" required="false" multi="false" array="false" longname="贷款期限" ref="GlDict.A.loan_term"/>
                <field id="data_sort" type="GlBaseType.U_LONG" required="false" multi="false" array="false" longname="数据序号" ref="GlDict.A.data_sort"/>
                <field id="gl_code" type="GlBaseType.U_SUBJECTNO" required="false" multi="false" array="false" longname="科目号" ref="GlDict.A.gl_code"/>
                <field id="debit_credit" type="GlEnumType.E_DEBITCREDIT" required="false" multi="false" array="false" longname="科目记账方向" ref="GlDict.A.debit_credit"/>
                <field id="amount_direction_sign" type="GlEnumType.E_AMOUNT_DIRECTION_SIGN" required="false" multi="false" array="false" longname="金额方向标志" ref="GlDict.A.amount_direction_sign"/>
                <field id="tax_separate_flag" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="增值税分离标识" ref="GlDict.A.tax_separate_flag"/>
