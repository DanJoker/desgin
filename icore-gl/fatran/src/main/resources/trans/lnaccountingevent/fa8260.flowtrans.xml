                <field id="scene_dimension_six" type="GlBaseType.U_BYTE40" required="false" multi="false" array="false" longname="场景维度6"/>
                <field id="scene_dimension_seven" type="GlBaseType.U_BYTE40" required="false" multi="false" array="false" longname="场景维度7"/>
                <field id="scene_dimension_eight" type="GlBaseType.U_BYTE40" required="false" multi="false" array="false" longname="场景维度8"/>
                <field id="scene_dimension_nine" type="GlBaseType.U_BYTE40" required="false" multi="false" array="false" longname="场景维度9"/>
                <field id="scene_dimension_ten" type="GlBaseType.U_BYTE40" required="false" multi="false" array="false" longname="场景维度10"/>
                <field id="scene_dimension_eleven" type="GlBaseType.U_BYTE40" required="false" multi="false" array="false" longname="场景维度11"/>
