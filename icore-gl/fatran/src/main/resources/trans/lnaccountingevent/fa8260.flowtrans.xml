            </fields>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvFaLnAccountingEvent.addLnAccountingEvent" id="addLnAccountingEvent" longname="增加场景核算事件定义">
            <in_mappings by_interface="true">
                <mapping src="scene_list" dest="addIn" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
