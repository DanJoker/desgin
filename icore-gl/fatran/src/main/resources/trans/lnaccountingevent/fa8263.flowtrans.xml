<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fa8263" longname="场景核算事件信息列表查询" kind="1" package="cn.sunline.edsp.busi.gl.fatran.trans.lnaccountingevent" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.edsp.busi.gl.fatran.trans.loanaccountingevent.intf">
        <input packMode="false">
            <field id="sys_no" type="GlBaseType.U_SYSNO" required="false" multi="false" array="false" longname="系统编号" ref="GlDict.A.sys_no"/>
            <field id="scene_code" type="GlBaseType.U_SCENE_CODE" required="false" multi="false" array="false" longname="场景编号" ref="GlDict.A.scene_code"/>
            <field id="product_code" type="GlBaseType.U_PRODUCT_CODE" required="false" multi="false" array="false" longname="产品编号" ref="GlDict.A.product_code"/>
            <field id="bal_type" type="GlBaseType.U_BAL_TYPE" required="false" multi="false" array="false" longname="金额类别" ref="GlDict.B.bal_type"/>
            <field id="loan_term" type="GlBaseType.U_LOAN_TERM" required="false" multi="false" array="false" longname="贷款期限" ref="GlDict.A.loan_term"/>
            <field id="gl_code" type="GlBaseType.U_SUBJECTNO" required="false" multi="false" array="false" longname="科目号" ref="GlDict.A.gl_code"/>
            <field id="effect_date" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="生效日期" ref="GlDict.A.effect_date"/>
            <field id="invalid_date" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="失效日期" ref="GlDict.A.invalid_date"/>
        </input>
        <output asParm="true" packMode="false">
            <field id="scene_list" type="TabFaParm.fap_accounting_prod_scene" required="false" multi="true" array="false" longname="产品核算事件信息输出"/>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvFaLnAccountingEvent.queryLnAccountingEvent" id="queryLnAccountingEvent" longname="查询场景核算事件定义">
            <in_mappings by_interface="true">
                <mapping src="bal_type" dest="queryIn.bal_type" by_interface="true" on_top="true"/>
                <mapping src="gl_code" dest="queryIn.gl_code" by_interface="true" on_top="true"/>
                <mapping src="loan_term" dest="queryIn.loan_term" by_interface="true" on_top="true"/>
                <mapping src="effect_date" dest="queryIn.effect_date" by_interface="true" on_top="true"/>
                <mapping src="invalid_date" dest="queryIn.invalid_date" by_interface="true" on_top="true"/>
                <mapping src="product_code" dest="queryIn.product_code" by_interface="true" on_top="true"/>
                <mapping src="scene_code" dest="queryIn.scene_code" by_interface="true" on_top="true"/>
                <mapping src="sys_no" dest="queryIn.sys_no" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="queryOut" dest="scene_list" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
