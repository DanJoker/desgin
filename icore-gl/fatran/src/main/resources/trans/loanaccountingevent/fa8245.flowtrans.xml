<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="fa8245" longname="贷款交易事件解析调试" kind="1" package="cn.sunline.ltts.busi.gl.fatran.trans.loanaccountingevent" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
