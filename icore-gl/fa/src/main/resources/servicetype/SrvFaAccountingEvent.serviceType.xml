<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="SrvFaAccountingEvent" kind="business" longname="会计核算事件维护" package="cn.sunline.ltts.busi.fa.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="addAccountingEvent" name="addAccountingEvent" longname="增加会计事件核算定义">
        <interface>
            <input packMode="false">
                <field id="addIn" type="ComFaParm.FaAccountingEventInfo" required="false" multi="false" array="false" longname="增加输入"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="mntAccountingEvent" name="mntAccountingEvent" longname="维护会计事件核算定义">
        <interface>
            <input packMode="false">
                <field id="modifyIn" type="ComFaParm.FaAccountingEventInfo" required="false" multi="false" array="false" longname="维护输入"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="delAccountingEvent" name="delAccountingEvent" longname="删除一条记录">
        <interface>
            <input packMode="false">
                <field id="sys_no" type="GlBaseType.U_SYSNO" required="false" multi="false" array="false" longname="系统编号" ref="GlDict.A.sys_no"/>
                <field id="accounting_alias" type="GlBaseType.U_BYTE32" required="false" multi="false" array="false" longname="核算别名" ref="GlDict.A.accounting_alias"/>
                <field id="bal_attributes" type="GlBaseType.U_BALPROPERTY" required="false" multi="false" array="false" longname="余额属性" ref="GlDict.A.bal_attributes"/>
                <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false"  longname="数据版本号" ref="BaseDict.Comm.recdver"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="queryEvent" name="queryEvent" longname="批量查询会计事件定义">
        <interface>
            <input packMode="false">
                <field id="queryIn" type="ComFaParm.FaAccountingEventInfo" required="false" multi="false" array="false" longname="查询输入"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="queryOut" type="ComFaParm.FaAccountingEventInfo" required="false" multi="true" array="false" longname="查询输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
