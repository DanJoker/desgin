            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="bookSetAccount" name="bookSetAccount" longname="套平入账">
        <interface>
            <input packMode="false">
                <field id="set_account_no" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="套号" ref="GlDict.A.set_account_no"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="mainOut" type="ComFaAccounting.FaSetAccountsMain" required="false" multi="false" array="false" longname="套账主表输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="delSetAccountDetail" name="delSetAccountDetail" longname="套账明细删除">
        <interface>
