            <input packMode="false">
                <field id="subject_accounting_ind" type="GlEnumType.E_YESORNO" required="false" multi="false" array="false" longname="科目记账标志" ref="GlDict.A.subject_accounting_ind"/>
                <field id="remark" type="GlBaseType.U_SHORTDESC" required="false" multi="false" array="false" longname="备注" ref="GlDict.A.remark"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="mainOut" type="ComFaAccounting.FaSetAccountsMain" required="false" multi="false" array="false" longname="套账主表返回"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="addSetAccountsDetails" name="addSetAccountsDetails" longname="套号记账明细录入">
