<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="SrvFaAccounting" kind="business" longname="记账类服务" package="cn.sunline.ltts.gl.fa.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="applySetAccountsNo" name="applySetAccountsNo" longname="申请套号">
