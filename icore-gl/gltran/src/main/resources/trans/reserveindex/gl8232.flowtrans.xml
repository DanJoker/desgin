<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="gl8232" longname="准备金缴存指标参数维护" kind="P" package="cn.sunline.ltts.busi.gltran.trans.reserveindex" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.gltran.trans.reserveindex.intf">
        <input packMode="true">
            <field id="ccy_code" type="BaseType.U_CRCYCD" required="true" multi="false" array="false" longname="货币代码" ref="GlDict.A.ccy_code"/>
            <field id="gl_code" type="GlBaseType.U_SUBJECTNO" required="true" multi="false" array="false" longname="科目号" ref="GlDict.A.gl_code"/>
            <field id="reserve_type" type="GlBusinessType.E_RESERVEAMTTYPE" required="true" multi="false" array="false" longname="准备金种类" ref="GlDict.A.reserve_type"/>
            <field id="calc_factor" type="GlBusinessType.E_CALCFACTOR" required="true" multi="false" array="false" longname="计算因子" ref="GlDict.A.calc_factor"/>
            <field id="paired_gl_code" type="GlBaseType.U_SUBJECTNO" required="true" multi="false" array="false" longname="配对科目" ref="GlDict.A.paired_gl_code"/>
            <field id="reserve_percent" type="BaseType.U_TRANAM" required="true" multi="false" array="false" longname="准备金比率" ref="GlDict.A.reserve_percent"/>
            <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false"  longname="数据版本号" ref="BaseDict.Comm.recdver"/>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvGlReserveIndex.mntReserveIndex" id="SrvGlReserveIndex.mntReserveIndex" longname="准备金缴存指标参数维护">
            <in_mappings by_interface="true">
                <mapping src="calc_factor" dest="modifyIn.calc_factor" by_interface="true" on_top="true"/>
                <mapping src="ccy_code" dest="modifyIn.ccy_code" by_interface="true" on_top="true"/>
                <mapping src="recdver" dest="modifyIn.recdver" by_interface="true" on_top="true"/>
                <mapping src="paired_gl_code" dest="modifyIn.paired_gl_code" by_interface="true" on_top="true"/>
                <mapping src="reserve_percent" dest="modifyIn.reserve_percent" by_interface="true" on_top="true"/>
                <mapping src="reserve_type" dest="modifyIn.reserve_type" by_interface="true" on_top="true"/>
                <mapping src="gl_code" dest="modifyIn.gl_code" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
        <!-- <service serviceName="SrvApTrxn.registerApsTransaction" id="SrvApTrxn.registerApsTransaction" longname="登记流水">
            <in_mappings by_interface="true"/>
            <out_mappings by_interface="true"/>
        </service> -->
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
