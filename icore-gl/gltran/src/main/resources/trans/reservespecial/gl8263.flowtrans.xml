<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="gl8263" longname="特殊对外缴存定义删除" kind="P" package="cn.sunline.ltts.busi.gltran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.gltran.trans.intf">
        <input packMode="true">
            <field id="branch_id" type="BaseType.U_BRCHNO" required="true" multi="false" array="false" longname="机构号" ref="GlDict.A.branch_id"/>
            <field id="ccy_code" type="BaseType.U_CRCYCD" required="true" multi="false" array="false" longname="货币代码" ref="GlDict.A.ccy_code"/>
            <field id="reserve_type" type="GlBusinessType.E_RESERVEAMTTYPE" required="true" multi="false" array="false" longname="准备金种类" ref="GlDict.A.reserve_type"/>
            <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false"  longname="数据版本号" ref="BaseDict.Comm.recdver"/>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvGlReserveSpecial.delGlReserveSpecialInfo" id="SrvGlReserveSpecial.delGlReserveSpecialInfo" longname="特殊对外缴存定义删除">
            <in_mappings by_interface="true">
                <mapping src="branch_id" dest="branch_id" by_interface="true" on_top="true"/>
                <mapping src="ccy_code" dest="ccy_code" by_interface="true" on_top="true"/>
                <mapping src="recdver" dest="recdver" by_interface="true" on_top="true"/>
                <mapping src="reserve_type" dest="reserve_type" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
        <!-- <service serviceName="SrvApTrxn.registerApsTransaction" id="SrvApTrxn.registerApsTransaction" longname="登记流水">
            <in_mappings by_interface="true"/>
            <out_mappings by_interface="true"/>
        </service> -->
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
