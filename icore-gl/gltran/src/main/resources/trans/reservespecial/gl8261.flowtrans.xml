        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvGlReserveSpecial.addGlReserveSpecialInfo" id="SrvGlReserveSpecial.addGlReserveSpecialInfo" longname="特殊对外缴存定义新增">
            <in_mappings by_interface="true">
                <mapping src="branch_id" dest="addIn.branch_id" by_interface="true" on_top="true"/>
                <mapping src="ccy_code" dest="addIn.ccy_code" by_interface="true" on_top="true"/>
                <mapping src="reserve_type" dest="addIn.reserve_type" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
