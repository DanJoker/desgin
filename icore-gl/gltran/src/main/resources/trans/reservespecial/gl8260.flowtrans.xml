<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="gl8260" longname="特殊对外缴存定义查询" kind="Q" package="cn.sunline.ltts.busi.gltran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.gltran.trans.intf">
        <input packMode="true">
            <field id="branch_id" type="BaseType.U_BRCHNO" required="true" multi="false" array="false" longname="机构号" ref="GlDict.A.branch_id"/>
            <field id="ccy_code" type="BaseType.U_CRCYCD" required="true" multi="false" array="false" longname="货币代码" ref="GlDict.A.ccy_code"/>
            <field id="reserve_type" type="GlBusinessType.E_RESERVEAMTTYPE" required="true" multi="false" array="false" longname="准备金种类" ref="GlDict.A.reserve_type"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="list01" required="false" multi="true" longname="查询输入" array="false">
                <field id="branch_id" type="BaseType.U_BRCHNO" required="true" multi="false" array="false" longname="机构号" ref="GlDict.A.branch_id"/>
                <field id="ccy_code" type="BaseType.U_CRCYCD" required="true" multi="false" array="false" longname="货币代码" ref="GlDict.A.ccy_code"/>
                <field id="reserve_type" type="GlBusinessType.E_RESERVEAMTTYPE" required="true" multi="false" array="false" longname="准备金种类" ref="GlDict.A.reserve_type"/>
                <field id="corpno" type="BaseType.U_CORPNO" required="true" multi="false" array="false" longname="法人代码" ref="ApDict.Aplt.corpno" />
