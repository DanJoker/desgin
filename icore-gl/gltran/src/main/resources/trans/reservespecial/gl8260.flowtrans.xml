        	</fields>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvGlReserveSpecial.queryGlReserveSpecialInfo" id="SrvGlReserveSpecial.queryGlReserveSpecialInfo" longname="特殊对外缴存定义查询">
            <in_mappings by_interface="true">
                <mapping src="branch_id" dest="queryIn.branch_id" by_interface="true" on_top="true"/>
                <mapping src="ccy_code" dest="queryIn.ccy_code" by_interface="true" on_top="true"/>
                <mapping src="reserve_type" dest="queryIn.reserve_type" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="queryOut" dest="list01" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
