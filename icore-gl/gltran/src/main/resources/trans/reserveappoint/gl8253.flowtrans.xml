<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="gl8253" longname="指定日期缴存定义删除" kind="P" package="cn.sunline.ltts.busi.gltran.trans.reserveappoint" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.gltran.trans.reserveappoint.intf">
        <input packMode="true">
            <field id="appoint_date" type="GlBaseType.U_DATE" required="false" multi="false" array="false" longname="指定日期" ref="GlDict.A.appoint_date"/>
            <field id="ccy_code" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="货币代码" ref="GlDict.A.ccy_code"/>
            <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false"  longname="数据版本号" ref="BaseDict.Comm.recdver"/>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvGlReserveAppoint.delGlReserveAppointInfo" id="SrvGlReserveAppoint.delGlReserveAppointInfo" longname="指定日期缴存定义删除">
            <in_mappings by_interface="true">
                <mapping src="appoint_date" dest="appoint_date" by_interface="true" on_top="true"/>
                <mapping src="ccy_code" dest="ccy_code" by_interface="true" on_top="true"/>
                <mapping src="recdver" dest="recdver" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
        <!-- <service serviceName="SrvApTrxn.registerApsTransaction" id="SrvApTrxn.registerApsTransaction" longname="登记流水">
            <in_mappings by_interface="true"/>
            <out_mappings by_interface="true"/>
        </service>-->
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
