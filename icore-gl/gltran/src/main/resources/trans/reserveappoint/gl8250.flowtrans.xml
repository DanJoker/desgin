<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="gl8250" longname="指定日期缴存定义查询" kind="Q" package="cn.sunline.ltts.busi.gltran.trans.reserveappoint" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.gltran.trans.reserveappoint.intf">
