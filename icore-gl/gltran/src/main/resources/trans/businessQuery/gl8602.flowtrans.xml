<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="gl8602" longname="折算汇率查询" kind="Q" package="cn.sunline.ltts.busi.gltran.trans.businessQuery" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.gltran.trans.businessQuery.intf">
        <input packMode="true">
            <field id="trxn_date" type="GlBaseType.U_DATE" required="false" multi="false" array="false" longname="transaction date" ref="GlDict.A.trxn_date"/>
            <field id="ccy_code" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="currency code" ref="GlDict.A.ccy_code"/>
            <field id="exchange_ccy_code" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="折合币种" ref="GlDict.A.exchange_ccy_code"/>
            <field id="exchange_rate_unit" type="GlBaseType.U_LONG" required="false" multi="false" array="false" longname="折算单位" ref="GlDict.A.exchange_rate_unit"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="list01" scope="GlExchange.GlExchangeRateInfo" required="false" multi="true" longname="查询输出" array="false">
                <field id="trxn_date" type="GlBaseType.U_DATE" required="false" multi="false" array="false" longname="交易日期" ref="GlDict.A.trxn_date"/>
                <field id="ccy_code" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="货币代码" ref="GlDict.A.ccy_code"/>
                <field id="exchange_ccy_code" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="折合币种" ref="GlDict.A.exchange_ccy_code"/>
                <field id="exchange_rate" type="GlBaseType.U_EXCHANGERATE" required="false" multi="false" array="false" longname="折算汇率" ref="GlDict.A.exchange_rate"/>
                <field id="exchange_rate_unit" type="GlBaseType.U_LONG" required="false" multi="false" array="false" longname="折算单位" ref="GlDict.A.exchange_rate_unit"/>
            </fields>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="SrvExchangB.selExchageRateInfo" id="SrvExchangB.selExchageRateInfo" longname="折算汇率查询">
            <in_mappings by_interface="true">
                <mapping src="ccy_code" dest="queryIn.ccy_code" by_interface="true" on_top="true"/>
                <mapping src="exchange_ccy_code" dest="queryIn.exchange_ccy_code" by_interface="true" on_top="true"/>
                <mapping src="exchange_rate_unit" dest="queryIn.exchange_rate_unit" by_interface="true" on_top="true"/>
                <mapping src="trxn_date" dest="queryIn.trxn_date" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="queryOut" dest="list01" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
