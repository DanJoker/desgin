<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="SrvGlReserveCycle" kind="business" longname="准备金缴存周期维护" package="cn.sunline.ltts.busi.gl.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="addReserveCycle" name="addReserveCycle" longname="增加一条缴存周期的信息">
        <interface>
            <input packMode="false">
                <field id="addIn" type="GlParm.GlReserveCycleInfo" required="false" multi="false" array="false" longname="新增信息输入"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="mntReserveCycle" name="mntReserveCycle" longname="维护一条缴存周期信息">
        <interface>
            <input packMode="false">
                <field id="modifyIn" type="GlParm.GlReserveCycleInfo" required="false" multi="false" array="false" longname="维护信息输入"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="delReserveCycle" name="delReserveCycle" longname="删除一条缴存周期信息">
        <interface>
            <input packMode="false">
                <field id="ccy_code" type="BaseType.U_CRCYCD" required="true" multi="false" array="false" longname="货币代码" ref="GlDict.A.ccy_code"/>
                <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false"  longname="数据版本号" ref="BaseDict.Comm.recdver"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="queryReserveCycleInfo" name="queryReserveCycleInfo" longname="查询缴存周期明细列表">
        <interface>
            <input packMode="false">
                <field id="queryIn" type="GlParm.GlReserveCycleInfo" required="false" multi="false" array="false" longname="查询信息输入"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="queryOut" type="GlParm.GlReserveCycleInfo" required="false" multi="true" array="false" longname="查询输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
