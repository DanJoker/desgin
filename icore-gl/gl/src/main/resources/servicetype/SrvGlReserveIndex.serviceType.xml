<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="SrvGlReserveIndex" kind="business" longname="准备金缴存指标维护" package="cn.sunline.ltts.busi.gl.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="addReserveIndex" name="addReserveIndex" longname="增加一条缴存指标信息">
        <interface>
            <input packMode="false">
                <field id="addIn" type="GlParm.GlReserveIndexInfo" required="false" multi="false" array="false" longname="增加输入"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="mntReserveIndex" name="mntReserveIndex" longname="维护一条缴存指标信息">
        <interface>
            <input packMode="false">
                <field id="modifyIn" type="GlParm.GlReserveIndexInfo" required="false" multi="false" array="false" longname="维护信息输入"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="delReserveIndex" name="delReserveIndex" longname="删除指定记录">
        <interface>
            <input packMode="false">
                <field id="ccy_code" type="BaseType.U_CRCYCD" required="true" multi="false" array="false" longname="货币代码" ref="GlDict.A.ccy_code"/>
                <field id="gl_code" type="GlBaseType.U_SUBJECTNO" required="true" multi="false" array="false" longname="科目号" ref="GlDict.A.gl_code"/>
                <field id="reserve_type" type="GlBusinessType.E_RESERVEAMTTYPE" required="true" multi="false" array="false" longname="准备金种类" ref="GlDict.A.reserve_type"/>
                <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false"  longname="数据版本号" ref="BaseDict.Comm.recdver"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="queryGlReserveIndexInfo" name="queryGlReserveIndexInfo" longname="查询缴存指标明细">
        <interface>
            <input packMode="false">
                <field id="queryIn" type="GlParm.GlReserveIndexInfo" required="false" multi="false" array="false" longname="查询输入"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="queryOut" type="GlParm.GlReserveIndexInfo" required="false" multi="true" array="false" longname="查询输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
