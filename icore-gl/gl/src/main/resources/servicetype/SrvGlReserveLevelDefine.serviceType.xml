<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="SrvGlReserveLevelDefine" kind="business" longname="准备金缴存层级定义维护" package="cn.sunline.ltts.busi.gl.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="addReserveLevelDefine" name="addReserveLevelDefine" longname="增加一条指定信息">
        <interface>
            <input packMode="false">
                <field id="addIn" type="GlParm.GlReserveLevelInfo" required="false" multi="false" array="false" longname="增加输入"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="mntReserveLevelDefine" name="mntReserveLevelDefine" longname="维护指定信息">
        <interface>
            <input packMode="false">
                <field id="modifyIn" type="GlParm.GlReserveLevelInfo" required="false" multi="false" array="false" longname="维护输入"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="delReserveLevelDefine" name="delReserveLevelDefine" longname="删除指定记录">
        <interface>
            <input packMode="false">
                <field id="deposit_level" type="GlBaseType.U_LONG" required="true" multi="false" array="false" longname="缴存层级" ref="GlDict.A.deposit_level"/>
                <field id="reserve_type" type="GlBusinessType.E_RESERVEAMTTYPE" required="true" multi="false" array="false" longname="准备金种类" ref="GlDict.A.reserve_type"/>
                <field id="deposit_busi_type" type="GlBusinessType.E_DEPOSITPAIDBUSIPROP" required="true" multi="false" array="false" longname="缴存款业务性质" ref="GlDict.A.deposit_busi_type"/>
                <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false"  longname="数据版本号" ref="BaseDict.Comm.recdver"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="queryGlReserveLevelInfo" name="queryGlReserveLevelInfo" longname="查询明细列表">
        <interface>
            <input packMode="false">
                <field id="queryIn" type="GlParm.GlReserveLevelInfo" required="false" multi="false" array="false" longname="查询输入"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="queryOut" type="GlParm.GlReserveLevelInfo" required="false" multi="true" array="false" longname="查询输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
