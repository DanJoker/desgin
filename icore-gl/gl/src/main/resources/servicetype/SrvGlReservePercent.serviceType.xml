<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="SrvGlReservePercent" kind="business" longname="准备金缴存比率" package="cn.sunline.ltts.busi.gl.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="addPercentInfo" name="addPercentInfo" longname="增加准备金缴存比率信息">
        <interface>
            <input packMode="false">
                <field id="addIn" type="GlParm.GlReservePercentInfo" required="false" multi="false" array="false" longname="增加输入"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="mntPercentInfo" name="mntPercentInfo" longname="修改准备金缴存比率信息">
        <interface>
            <input packMode="false">
                <field id="modifyIn" type="GlParm.GlReservePercentInfo" required="false" multi="false" array="false" longname="维护输入"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="delPercentInfo" name="delPercentInfo" longname="删除一条准备金比率信息">
        <interface>
            <input packMode="false">
                <field id="branch_id" type="BaseType.U_BRCHNO" required="true" multi="false" array="false" longname="机构号" ref="GlDict.A.branch_id"/>
                <field id="ccy_code" type="BaseType.U_CRCYCD" required="true" multi="false" array="false" longname="货币代码" ref="GlDict.A.ccy_code"/>
                <field id="reserve_type" type="GlBusinessType.E_RESERVEAMTTYPE" required="true" multi="false" array="false" longname="准备金种类" ref="GlDict.A.reserve_type"/>
                <field id="recdver" type="BaseType.U_RECDVER" required="true" multi="false" array="false"  longname="数据版本号" ref="BaseDict.Comm.recdver"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="queryReservePercentInfo" name="queryReservePercentInfo" longname="查询准备金比率明细信息">
        <interface>
            <input packMode="false">
                <field id="queryIn" type="GlParm.GlReservePercentInfo" required="false" multi="false" array="false" longname="查询输入"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="queryOut" type="GlParm.GlReservePercentInfo" required="false" multi="true" array="false" longname="查询输出"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
