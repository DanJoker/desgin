            <field id="gnstyp" longname="路由映射类型" type="ApGnsType.U_GNSTYP" ref="ApGnsDict.Gns.gnstyp" primarykey="false" final="false" nullable="false" identity="false" allowSubType="true"/>
            <field id="gnsopt" longname="路由映射识别标志" type="ApGnsEnumType.E_GNSOPT" ref="ApGnsDict.Gns.gnsopt" primarykey="false" final="false" nullable="false" identity="false" allowSubType="true"/>
