<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="ApGns" package="cn.sunline.ltts.gns" longname="路由映射表" classgen="normal" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
