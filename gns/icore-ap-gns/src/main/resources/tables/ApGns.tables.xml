        </fields>
        <ddls/>
        <odbindexes>
            <index id="odb1" type="unique" fields="gnskey gnstyp gnsopt gnschn" operate="deleteOne selectOne updateOne"/>
            <index id="ddb2" type="index" fields="gnskey gnstyp gnsopt" operate="selectFirst"/>
